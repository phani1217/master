+/**
+  * @brief  Intialize Ethernet module
+  * @param  p_obj : Pointer to structure that contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @retval TXZ_SUCCESS :Success.
+  * @retval TXZ_ERROR   :Failure.
+  */
+/*--------------------------------------------------*/
+
+TXZ_Result ethm_init(ethm_t *p_obj)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+	uint16_t timeout = 0;
+
+	p_EthmObj = p_obj;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+    /* Enable clock settings for the ports */
+    ethm_clock_settings_enable();
+
+    /* Initialize ethernet module I/O ports */
+    ethm_port_initialize();
+
+    /* Initialize timer for timeout mechanism */
+    ethm_timer_init();
+
+    /* Select MII */
+    p_obj->p_instance->MAC_CTRL0 = MAC0_PHY_INTF_SEL_MII;
+
+    /* Clock supply to PHY interface MII/RMII */
+    p_obj->p_instance->MAC_CTRL1 = MAC_CTRL1_RXCLKEN | MAC_CTRL1_TXCLKEN | MAC_CTRL1_RMIICLKEN;
+
+    /* Reset Ethernet Module */
+    p_obj->p_instance->MAC_CTRL2 = MAC_CTRL2_SRST;
+
+    /* Perform Software reset */
+    /* Setting this bit to "1" resets the TMAC, TDMA block and all registers in the ETHM */
+    TSB_EMA_BUS_MODE_SWR = SWR_RESET;
+
+	do
+	{
+
+		ethm_mdelay(SWR_DELAY);
+		timeout += TIMEOUT_INCR;
+
+		/* check for timeout occurance */
+		if (timeout > SWR_TIMEOUT) {
+
+			/* Timeout error */
+			return TXZ_ERROR;
+		}
+	}while (TSB_EMA_BUS_MODE_SWR);
+
+    /* Set CSR clock range */
+    /* system Core clock frequency is 192MHz */
+    p_obj->p_instance->MII_ADDRESS = MII_ADRR_CR_Div42;
+
+    /* Start Ethernet phy */
+    if (ethm_phy_start(p_obj, TMPM4NR_PHY_ADDR) != TXZ_SUCCESS) {
+        return TXZ_ERROR;
+    }
+
+    /* Initialize DMA */
+    if (ethm_dma_init(p_obj) != TXZ_SUCCESS) {
+        return TXZ_ERROR;
+    }
+
+    /* Initialize EMAC */
+    if (ethm_emac_init(p_obj) != TXZ_SUCCESS) {
+        return TXZ_ERROR;
+    }
+
+    /* Disable Interrupt */
+    NVIC_DisableIRQ(INTEMA_IRQn);
+    /* Enable Interrupt */
+    NVIC_EnableIRQ(INTEMA_IRQn);
+	}
+    return result;
+}
