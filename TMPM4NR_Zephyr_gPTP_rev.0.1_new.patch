+				reg = <0x400e0000 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PA>;
+			};
+
+			pb: gpio@400e0100 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0100 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PB>;
+			};
+
+			pc: gpio@400e0200 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0200 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PC>;
+			};
+
+			pd: gpio@400e0300 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0300 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PD>;
