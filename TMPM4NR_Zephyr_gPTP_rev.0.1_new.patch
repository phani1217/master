+/*--------------------------------------------------*/
+/**
+  * @brief  Configures and initialize EMAC.
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance.
+  * @retval TXZ_SUCCESS :Success.
+  * @retval TXZ_ERROR   :Failure.
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_emac_init(ethm_t *p_obj)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+    TSB_EM_TypeDef *p_ethm;
+    TSB_ETHM_MAC_Config_TypeDef mac_config;
+    __IO uint32_t tmpreg = 0U;
+    uint8_t MACAddr[6];
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Ethernet MAC Initilization */
+	    mac_config.SARC  = MAC_CONF_SA_ALL_FRAMES;
+	    mac_config.LM = MAC_CONF_LOOPBACK_MODE_DISABLE;
+	    mac_config.RA = MAC_FRAME_FLTR_RECEIVE_ALL_FRAMES_DISABLE;
+	    mac_config.SAF = MAC_FRAME_FLTR_SA_FILTER_DISABLE;
+	    mac_config.PCF = MAC_FRAME_FLTR_PASS_CONTROL_FRAMES;
+	    mac_config.DBF = MAC_FRAME_FLTR_PASS_ALL_BROADCAST_FRAMES_ENABLE;
+	    mac_config.DAIF = MAC_FRAME_FLTR_DESTINATION_ADDR_FILTER_NORMAL;
+	    mac_config.PR = MAC_FRAME_FLTR_PROMISCUOUS_MODE_DISABLE;
+	    mac_config.HMC = MAC_FRAME_FLTR_HASH_MULTICAST_FRAMES_FILTERING;
+	    mac_config.HUC = MAC_FRAME_FLTR_HASH_UNICAST_FRAMES_FILTERING;
+	    mac_config.HTH = 0x0U;
+	    mac_config.HTL = 0x0U;
+		mac_config.PM = MAC_FRAME_FLTR_ALL_MULTICAST_FRAMES_FILTERING;
+
+	    tmpreg = p_ethm->TMAC_CONFIGURATION;
+
+	    tmpreg |= (uint32_t)mac_config.SARC |
+	              MAC_CONF_FULL_DUPLEX_MODE |
+	              MAC_CONF_TRANSFER_SPEED_100M;
+
+
+	    /* Write to TMAC Configuration Register Register */
+	    p_ethm->TMAC_CONFIGURATION = (uint32_t)tmpreg;
+
+	    tmpreg = p_ethm->TMAC_FRAME_FILTER;
+
+	    tmpreg |= (uint32_t)(mac_config.RA |
+	                         mac_config.SAF |
+	                         mac_config.PCF |
+	                         mac_config.DBF |
+	                         mac_config.DAIF |
+	                         mac_config.PR |
+	                         mac_config.HMC |
+	                         mac_config.HUC | 	
+	                         mac_config.PM);
+
+	    /* Write to TMAC Frame Filter Register */
+	    p_ethm->TMAC_FRAME_FILTER  = (uint32_t)tmpreg;
+
+	    /* Write to Hash Table Top Register */
+	    p_ethm->HASH_TABLE_HIGH = (uint32_t)mac_config.HTH;
+
+	    /* Write to Hash Table Lower Register */
+	    p_ethm->HASH_TABLE_LOW  = (uint32_t)mac_config.HTL;
+
+
+
+	    /* Write to Flow Control Register */
+	    p_ethm->FLOW_CONTROL = 0x00000000;
+
+	    /* Set MAC address */
+	    MACAddr[0] = MAC_ADDR_0;
+	    MACAddr[1] = MAC_ADDR_1;
+	    MACAddr[2] = MAC_ADDR_2;
+	    MACAddr[3] = MAC_ADDR_3;
+	    MACAddr[4] = MAC_ADDR_4;
+	    MACAddr[5] = MAC_ADDR_5;
+
+	    /* Write to MAC address 0 Top Register and MAC address 0 lower register */
+	    if (ethm_set_mac_address(p_obj,&MACAddr[0]) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Enable MAC transmission */
+	    TSB_EMA_TMAC_CONFIGURATION_TE = 1;
+
+	    /* Enable MAC Reception */
+	    TSB_EMA_TMAC_CONFIGURATION_RE = 1;
+	}
+
+    return result;
+}
