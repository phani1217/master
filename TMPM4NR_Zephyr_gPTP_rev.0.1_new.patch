+
+			pe: gpio@400e0400 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0400 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PE>;
+			};
+
+			pf: gpio@400e0500 {
