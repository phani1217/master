+/*--------------------------------------------------*/
+/**
+  * @brief  Writes to physical register
+  * @param  p_ethm  : Ethernet instance
+  * @param  phy_addr : physical layer address
+  * @param  reg_addr : physical register address
+  * @param  data    : data to write
+  * @retval TXZ_SUCCESS :Success
+  * @retval TXZ_ERROR   :Failure
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_phy_write(TSB_EM_TypeDef *p_ethm, uint8_t phy_addr, uint8_t reg_addr, uint16_t data)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+    uint16_t timeout =0;
+    __IO uint32_t tmpreg = 0U;
+
+    /* Check the parameter.           */
+	if ((void *)(p_ethm) == (void *)0) {
+		 result = TXZ_ERROR;
+	 } else {
+
+	    /* Check for MII busy status, wait for MII not busy until timeout occurs */
+	    while (ethm_read_register((void *)&p_ethm->MII_ADDRESS) & MII_ADR_GB) {
+
+			/* wait */
+			ethm_mdelay(PHY_TIME_DELAY);
+
+	        timeout += TIMEOUT_INCR;
+
+			/* check for timeout occurance */
+	        if (timeout > PHY_IO_TIMEOUT) {
+
+				/* Timeout error */
+	            return TXZ_ERROR;
+	        }
+	    }
+
+	    /* store MII Data */
+	    ethm_write_register((uint32_t) data, (void *)&p_ethm->MII_DATA);
+
+		tmpreg = MII_ADRR_PHYADR(phy_addr) | MII_ADRR_PHYREG(reg_addr) | MII_ADRR_WRITE | MII_ADR_GB;
+
+	    /* Write to physical register at specified address and perform MII write operation */
+	    ethm_write_register(tmpreg, (void *)(&p_ethm->MII_ADDRESS));
+
+		/* Reset time out */
+	    timeout = 0;
+
+	    /* Check for MII busy status, wait for MII not busy until timeout occurs*/
+	    while (ethm_read_register((void *)&p_ethm->MII_ADDRESS) & MII_ADR_GB) {
+
+
+	        ethm_mdelay(PHY_TIME_DELAY);
+
+	        timeout += TIMEOUT_INCR;
+
+			/* check for timeout occurance */
+	        if (timeout > PHY_IO_TIMEOUT) {
+
+				/* Timeout error */
+	            return TXZ_ERROR;
+	        }
+	    }
+	}
+    return result;
+
+}
