+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0500 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PF>;
+			};
+
+			pg: gpio@400e0600 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0600 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PG>;
