+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0700 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PH>;
+			};
+
+			pj: gpio@400e0800 {
+				compatible = "toshiba,txzp-gpio";
+				reg = <0x400e0800 0x100>;
+				gpio-controller;
+				#gpio-cells = <2>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PJ>;
+			};
+
+			pk: gpio@400e0900 {
+				compatible = "toshiba,txzp-gpio";
+				reg = <0x400e0900 0x100>;
+				gpio-controller;
+				#gpio-cells = <2>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PK>;
