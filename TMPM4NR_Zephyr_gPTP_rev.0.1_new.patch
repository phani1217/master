+	struct k_thread rx_thread;
+	bool link_up;
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	const struct device *ptp_clock;
+	float clk_ratio;
+	float clk_ratio_adj;
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+};
