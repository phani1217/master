+/*--------------------------------------------------*/
+/**
+  * @brief  Initialize the DMA Receive descriptors.
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @retval TXZ_SUCCESS :Success.
+  * @retval TXZ_ERROR   :Failure.
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_dma_rxdesc_init(ethm_t *p_obj)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+	uint32_t index = 0;
+    uint8_t *p_rxBuff;
+    TSB_ETHM_DMADesc_TypeDef *p_dmarxdesc,*p_dmarxdesc_next;
+	TSB_EM_TypeDef *p_ethm;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /*Assign to receive buffer base address */
+	    p_rxBuff = &ethm_rxBuffer[0][0];
+
+	    /* Set the Ethernet RxDesc pointer with the first one of the p_dmarxdesc list */
+	    p_obj->p_rx_desc = ethm_rxDescriptor;
+
+	    /* Loop all descriptors and assign valid buffer address and  descriptor address */
+	    for (index = 0; index < ETHM_RX_DESCRIPTOR_NUM; index++) {
+	        /* Assign temp pointer to descriptor present at index location */
+	        p_dmarxdesc = p_obj->p_rx_desc + index;
+			p_dmarxdesc_next = p_dmarxdesc + 1;
+
+	        /* Set Second Address Chain bit. Indicates second address of the descriptor is the next descriptor address */
+	        p_dmarxdesc->des0 = RX_DMA_DESC0_OWN;
+
+	        /* Assign Send buffer 1 address pointer to transmit buffer */
+	        p_dmarxdesc->des1 = RX_DMA_DESC1_RCH | ETHM_RX_BUF_SIZE;
+
+	        /* Set Buffer1 address pointer */
+	        p_dmarxdesc->des2_Buffer_1_Addr = (uint32_t)(&p_rxBuff[index * ETHM_RX_BUF_SIZE]);
+
+	        /* Assign descriptor address to Next descriptor address */
+	        if (index < ETHM_LAST_RX_DESCRIPTOR) {
+	            /* If current descriptor is not last descriptor,
+					write next descriptor address value to next descriptor base address */
+	            p_dmarxdesc->des3_Next_Desc_Addr = (uint32_t)p_dmarxdesc_next;
+	        } else {
+	            /* If current descriptor is last descriptor,
+					write next descriptor address register to the first descriptor base address */
+	            p_dmarxdesc->des3_Next_Desc_Addr = (uint32_t)(p_obj->p_rx_desc);
+	        }
+
+	    }
+
+	    /* Set Receive Descriptor List Address Register */
+	    p_ethm->RECEIVE_DESCRIPTOR_LIST_ADDRESS = (uint32_t) ethm_rxDescriptor;
+	}
+    return result;
+}
