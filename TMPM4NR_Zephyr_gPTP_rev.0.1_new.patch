+#ifndef ZEPHYR_INCLUDE_DT_BINDINGS_PINCTRL_TXZP_PINCTRL_H_
+#define ZEPHYR_INCLUDE_DT_BINDINGS_PINCTRL_TXZP_PINCTRL_H_
+
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define TXZP_PORT_NUM_POS  8
+#define TXZP_PORT_NUM_MASK 0x1f
+
+#define TXZP_PIN_NUM_POS  0
+#define TXZP_PIN_NUM_MASK 0xf
+
+#define TXZP_FUNC_POS 16
+#define TXZP_FUNC_MASK 0xf
+
+#define TXZP_PINMUX_M4N_A2I(_n)	\
+		(_n - 'A' - ((_n) < 'I' ? 0: \
+					((_n) < 'O' ? 1: \
+					((_n) < 'Q' ? 2: \
+					((_n) < 'S' ? 3: \
+					((_n) < 'X' ? 4: 5))))))
+
+/**
+ * @param port Port name ('A' to 'Y')
+ * @param pin Port pin number (0 to 7)
+ * @param mux function number (1 to 8)
+ */
+#define TXZP_PINMUX(_port, _pin, _function)	\
+		((((TXZP_PINMUX_M4N_A2I(_port)) & TXZP_PORT_NUM_MASK) << TXZP_PORT_NUM_POS)|	\
+		(((_pin) & TXZP_PIN_NUM_MASK) << TXZP_PIN_NUM_POS)|			\
+		(((_function) & TXZP_FUNC_MASK) << TXZP_FUNC_POS))
+
+
+
