+/*--------------------------------------------------*/
+/**
+  * @brief  Enable clock settings for the ports
+  * @param  -
+  * @retval -
+  */
+/*--------------------------------------------------*/
+static void ethm_clock_settings_enable(void)
+{
+
+    TSB_CG_FSYSMENA_IPMENA23 = 1;     /* UART  0   */
+
+    /* Enable Clock for ETHM */
+    TSB_CG_FSYSMENC_IPMENC10 = TXZ_ENABLE;
