+/*--------------------------------------------------*/
+/**
+  * @brief  Configuration and Initialization of Physical layer transceiver
+  * @param  p_obj : Structure handle contains ethernet module instance
+  * @param  addr : Physical layer address
+  * @retval TXZ_SUCCESS :Success
+  * @retval TXZ_ERROR   :Failure
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_phy_start(ethm_t *p_obj, uint8_t addr)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+    uint16_t bsr_reg, bcr_reg, timeout = 0;
+    TSB_EM_TypeDef *p_ethm;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Read Physical link register value */
+	    if (ethm_phy_read(p_ethm, addr, TMPM4NR_PHY_BCR, &bcr_reg) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Perform PHY Software reset  */
+	    if (ethm_phy_write(p_ethm, addr, TMPM4NR_PHY_BCR, TMPM4NR_PHY_RESET) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Time out Delay to assure PHY reset */
+	    ethm_mdelay(PHY_RESET_TIMEOUT_DELAY);
+
+	    /* Wait for Link Status is up */
+	    do {
+	        if (ethm_phy_read(p_ethm, addr, TMPM4NR_PHY_BSR, &bsr_reg) != TXZ_SUCCESS) {
+	            return TXZ_ERROR;
+	        }
+
+			/* Wait */
+	        ethm_mdelay(PHY_TIME_DELAY);
+
+	        timeout += TIMEOUT_INCR;
+
+			/* check for timeout occurance */
+	        if (timeout > PHY_IO_TIMEOUT) {
+				/* Timeout error */
+	            return TXZ_ERROR;
+	        }
+
+	    } while (((bsr_reg & TMPM4NR_PHY_BSR_LINK) != TMPM4NR_PHY_BSR_LINK));
+
+	    bsr_reg = 0;
+
+	    if (ethm_phy_read(p_ethm, addr, TMPM4NR_PHY_BSR, &bsr_reg) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Full duplex, 100 Mbps */
+	    if (ethm_phy_read(p_ethm, addr, TMPM4NR_PHY_BCR, &bcr_reg) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Set values for Full duplex and speed 100 Mbps */
+	    bcr_reg |= TMPM4NR_PHY_BCR_DUPLEX | TMPM4NR_PHY_BSR_100HDX;
+
+	    /* Write PHY register to perform Full duplex and speed 100 Mbps */
+	    if (ethm_phy_write(p_ethm, addr, TMPM4NR_PHY_BCR, bcr_reg) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+	}
+    return result;
+}
