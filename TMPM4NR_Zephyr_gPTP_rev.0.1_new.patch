+/**
+  * @brief Time out  delay
+  * @param msec : time out in milli second
+  * @retval -
+  */
+/*--------------------------------------------------*/
+static void ethm_mdelay(uint32_t msec)
+{
+
+    uint32_t to = msec * TIMEOUT_1MSEC;
+    uint32_t val;
+    val = (TSB_T32A2->TMRC);
+    uint32_t cur;
+    cur = (TSB_T32A2->TMRC);
+    while ((cur - val) < to) {
+        cur = (TSB_T32A2->TMRC);
+    }
+
+}
