+ *
+ * SPDX-License-Identifier: Apache-2.0
+ *
+ */
+
+#include <zephyr/kernel.h>
+#include <zephyr/device.h>
+#include <zephyr/init.h>
+
+/**
+ *
+ * @brief Perform basic hardware initialization
+ *
+ * @return 0
+ */
+
+static int soc_init(const struct device *arg)
+{
+	ARG_UNUSED(arg);
+
+	/* Install default handler that simply resets the CPU
+	 * if configured in the kernel, NOP otherwise
+	 */
+	NMI_INIT();
+	return 0;
+}
+
+/* Configure Warm-up time */
+#define EXTALH                  12000000U        /* External high-speed oscillator freq */
+#define IXTALH                  10000000U        /* Internal high-speed oscillator freq */
+#define HZ_1M                  (1000000U)
+#define WU_TIME_EXT            (5000U)          /* warm-up time for EXT is 5ms   */
+#define INIT_TIME_PLL          (100U)           /* Initial time for PLL is 100us */
+#define LOCKUP_TIME_PLL        (400U)           /* Lockup time for PLL is 400us  */
+#define WUPHCR_WUPT_EXT        ((((WU_TIME_EXT * IXTALH / HZ_1M) - 16U) /16U) << 20U) /* OSCCR<WUPT11:0> = (warm-up time(us) * IXTALH - 16) / 16 */
+#define WUPHCR_INIT_PLL      ((((INIT_TIME_PLL * EXTALH / HZ_1M) - 16UL) /16UL) << 20U)
+#define WUPHCR_LUPT_PLL      ((((LOCKUP_TIME_PLL * EXTALH / HZ_1M) - 16UL) /16UL) << 20U)
+#define EXT_CG_WUPHCR_WUCLK_SET              (0x00000000U)  /* WUCLK for External HOSC select the IHOSC1 */
+#define CG_WUPHCR_WUCLK_SET              (0x00000100U)  /* WUCLK for External HOSC select the IHOSC1 */
+
+static void wdt_disable(void)
+{
+	volatile uint32_t *siwdt = (volatile uint32_t *)(uintptr_t)0x400a0600;
+
+	siwdt[1] = 0x00;
+	siwdt[2] = 0xb1;
+}
+
+void z_arm_platform_init(void)
+{
+	wdt_disable();
+}
+
+SYS_INIT(soc_init, PRE_KERNEL_1, CONFIG_KERNEL_INIT_PRIORITY_DEFAULT);
