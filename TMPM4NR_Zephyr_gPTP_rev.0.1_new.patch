diff --git a/drivers/clock_control/clock_control_txzp_cg.c b/drivers/clock_control/clock_control_txzp_cg.c
new file mode 100644
index 0000000000..771a38e76c
--- /dev/null
+++ b/drivers/clock_control/clock_control_txzp_cg.c
@@ -0,0 +1,383 @@
+/*
+ * Copyright (c) 2023 Toshiba Electronic Devices & Storage Corporation
+ *
+ * TXZ+ Clock Generator / Module Standby and Software Reset
+ *
+ * SPDX-License-Identifier: Apache-2.0
+ */
+
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define DT_DRV_COMPAT toshiba_txzp_cg
+#define LOG_LEVEL CONFIG_CLOCK_CONTROL_LOG_LEVEL
+
+/*------------------------------------------------------------------------------*/
+/*  Includes                                                                    */
+/*------------------------------------------------------------------------------*/
+#include <errno.h>
+#include <zephyr/arch/cpu.h>
+#include <zephyr/drivers/clock_control.h>
+#include <zephyr/dt-bindings/clock/txzp_cg.h>
+#include <soc.h>
+#include <zephyr/logging/log.h>
+
+LOG_MODULE_REGISTER(clock_control_txzp);
+
+/**
+ * @brief Clock control configuration structure definition
+ */
+struct txzp_cg_config {
+	uint32_t base_addr;
+	uint32_t clocksel;
+	uint32_t extfreq;
+	uint32_t osc;
+	uint32_t eosc;
+	uint32_t gear;
+	uint32_t mcksel;
+	uint32_t prck;
+	uint32_t standby;
+	uint32_t pll0;
+};
+
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define TXZP_CGPROTECT 0x00
+#define TXZP_CGOSCCR   0x04
+#define OSCSEL_INT     0x00000000
+#define OSCSEL_EXT     0x00000100
+#define OSCF_INT       0x00000000
+#define OSCF_EXT       0x00000200
+#define IHOSC1_DIS     0x00000000
+#define IHOSC1_ENA     0x00000001
+#define EOSCEN_DIS     0x00000000
+#define EOSCEN_EOSC    0x00000002
+#define EOSCEN_ECLK    0x00000004
+#define IHOSC1F_DIS    0x00000000
+#define IHOSC1F_ENA    0x00010000
+
+#define TXZP_CGSYSCR 0x08
+#define GEAR_X1	     0x00000000
+#define GEAR_1_2     0x00000001
+#define GEAR_1_4     0x00000002
+#define GEAR_1_8     0x00000003
+#define GEAR_1_16    0x00000004
+#define MCKSEL(mck)  ((mck) << 6U)
+#define MCKSEL_X1    0x00000000
+#define MCKSEL_1_2   0x00000001
+#define MCKSEL_1_4   0x00000002
+#define PRCK(prck)   ((prck) << 8U)
+#define PRCK_X1	     0x00000000
+#define PRCK_1_2     0x00000001
+#define PRCK_1_4     0x00000002
+#define PRCK_1_8     0x00000003
+#define PRCK_1_16    0x00000004
+#define PRCK_1_32    0x00000005
+#define PRCK_1_64    0x00000006
+#define PRCK_1_128   0x00000007
+#define PRCK_1_256   0x00000008
+#define PRCK_1_512   0x00000009
+#define GEARST_SHIFT 16U
+#define MCKGST_SHIFT 22U
+#define PRCKST_SHIFT 24U
+#define MCKPST_SHIFT 30U
+#define GEARST(st)   ((st) << GEARST_SHIFT)
+#define MCKGST(st)   ((st) << MCKGST_SHIFT)
+#define PRCKST(st)   ((st) << PRCKST_SHIFT)
+#define MCKPST(st)   ((st) << MCKPST_SHIFT)
+#define GEARST_MASK  GEARST(0x7)
+#define MCKGST_MASK  MCKGST(0x3)
+#define PRCKST_MASK  PRCKST(0xf)
+#define MCKPST_MASK  MCKPST(0x3)
+#define MCKST_MASK   (MCKPST(0x3) | MCKGST(0x3))
+#define MCKST(st)    (MCKPST(st) | MCKGST(st))
+
+#define TXZP_CGSTBYCR 0x0c
+#define STBY_IDLE     0x00000000
+#define STBY_STOP1    0x00000001
+#define STBY_STOP2    0x00000002
+
+#define TXZP_CGPLL0SEL	    0x20
+#define PLL0ON_STOP	    0x00000000
+#define PLL0ON_START	    0x00000001
+#define PLL0SEL_FOSC	    0x00000000
+#define PLL0SEL_FPLL	    0x00000002
+#define PLL0ST_FPLL	    0x00000004
+#define CG_8Mx24_FPLL	    (0x00245030UL << 8U) /* fPLL = 8MHz  * 24       */
+#define CG_12Mx16_FPLL	    (0x0036D020UL << 8U) /* fPLL = 12MHz * 16       */
+#define CG_16Mx12_FPLL	    (0x00495018UL << 8U) /* fPLL = 16MHz * 12       */
+#define CG_24Mx8_FPLL	    (0x006D9010UL << 8U) /* fPLL = 24MHz * 8        */
+#define CG_10Mx19_1875_FPLL (0x002E9626UL << 8U) /* fPLL = 10MHz * 19.1875  */
+
+#define WU_TIME_EXT	(5000U) /* warm-up time for EXT is 5ms   */
+#define INIT_TIME_PLL	(100U)	/* Initial time for PLL is 100us */
+#define LOCKUP_TIME_PLL (400U)	/* Lockup time for PLL is 400us  */
+#define WUPHCR_WUPT_EXT(freq)                                                                      \
+	((((WU_TIME_EXT * (freq)) - 16U) / 16U)                                                    \
+	 << 20U) /* OSCCR<WUPT11:0> = (warm-up time(us) * IXTALH - 16) / 16 */
+#define WUPHCR_INIT_PLL(freq) ((((INIT_TIME_PLL * (freq)) - 16UL) / 16UL) << 20U)
+#define WUPHCR_LUPT_PLL(freq) ((((LOCKUP_TIME_PLL * (freq)) - 16UL) / 16UL) << 20U)
+
+#define TXZP_CGWUPHCR 0x30
+#define WUON_START    0x00000001
+#define WUEF_END      0x00000000
+#define WUEF_WUP      0x00000002
+#define WUCLK_IHOSC1  0x00000000
+#define WUCLK_EHOSC   0x00000100
+
+#define TXZP_CFWUPLCR  0x34
+#define TXZP_CGSYSMENC 0x44
+#define TXZP_CGSYSMENA 0x48
+#define TXZP_CGSYSMENB 0x4c
+#define TXZP_CGSYSENA  0x50
+#define TXZP_CGFCEN    0x58
+#define TXZP_CGSPCLKEN 0x5c
+#define TXZP_CGEXTEND2 0x68
+
+static const uint8_t txzp_cg_reg_map[] = {
+	TXZP_CGSYSMENC, TXZP_CGSYSMENA, TXZP_CGSYSMENB, TXZP_CGSYSENA, TXZP_CGFCEN,
+};
+
+/**
+ * @brief Reads value from memory address
+ * @param addr memory address
+ * @return value
+ */
+static int txzp_cg_read(uint32_t addr)
+{
+	return sys_read32(addr);
+}
+
+/**
+ * @brief Writes value at memory address
+ * @param addr: memory address
+ * @param val: value to set at @addr
+ */
+static void txzp_cg_write(uint32_t addr, uint32_t val)
+{
+	sys_write32(val, addr);
+}
+
+/**
+ * @brief  Set/Clear bits at memory address.
+ * @param  addr: memory address
+ * @param  bits: bits to set or clear
+ * @param  on: set the bits if true else clear
+ */
+static void txzp_cg_set_bits(uint32_t addr, uint32_t bits, bool on)
+{
+	uint32_t reg_val = txzp_cg_read(addr);
+
+	if (on)
+		reg_val |= bits;
+	else
+		reg_val &= ~bits;
+	txzp_cg_write(addr, reg_val);
+}
+
+/**
+ * @brief Enable or disable the clock controlled by device
+ * @param dev Clock Control device driver instance
+ * @param sys Data representing the clock
+ * @param on Enables the clock if true else disable
+ * @return 0 Success
+ * @return else Error
+ */
+static int txzp_cg_onoff(const struct device *dev, clock_control_subsys_t sys, bool on)
+{
+	const struct txzp_cg_config *config = dev->config;
+	uint32_t base_address = config->base_addr;
+	uint32_t clkid = *(uint32_t *)sys;
+	uint32_t reg = clkid >> 8;
+	uint32_t bit = clkid & 0xff;
+	uint32_t bitmask = BIT(bit);
+	uint32_t off = (uint32_t)txzp_cg_reg_map[reg];
+
+	txzp_cg_set_bits(base_address + off, bitmask, on);
+
+	return 0;
+}
+
+/**
+ * @brief Initializes the clock controlled by device
+ * @param dev Clock Control device driver instance
+ * @return 0 success
+ * @return else error
+ */
+static int txzp_cg_init(const struct device *dev)
+{
+	const struct txzp_cg_config *config = dev->config;
+	uint32_t fpll;
+	uint32_t wuclk_set;
+	uint32_t addr = config->base_addr;
+
+	if (config->clocksel) {
+		/* external clock setup */
+		txzp_cg_write(addr + TXZP_CGSYSCR, config->gear);
+		txzp_cg_write(addr + TXZP_CGWUPHCR, WUPHCR_WUPT_EXT(config->extfreq) | WUCLK_EHOSC);
+		txzp_cg_set_bits(addr + TXZP_CGOSCCR, EOSCEN_EOSC, true);
+		txzp_cg_write(addr + TXZP_CGWUPHCR,
+			      WUPHCR_WUPT_EXT(config->extfreq) | WUCLK_EHOSC | WUON_START);
+		while ((txzp_cg_read(addr + TXZP_CGWUPHCR) & WUEF_WUP)) {
+			;
+		}
+
+		txzp_cg_set_bits(addr + TXZP_CGOSCCR, OSCSEL_EXT, true);
+		while (!(txzp_cg_read(addr + TXZP_CGOSCCR) & OSCF_EXT)) {
+			;
+		}
+		txzp_cg_set_bits(addr + TXZP_CGOSCCR, IHOSC1_ENA, false);
+		wuclk_set = WUCLK_EHOSC;
+	} else
+		wuclk_set = WUCLK_IHOSC1;
+
+	txzp_cg_write(addr + TXZP_CGSYSCR, MCKSEL(config->mcksel) | config->gear);
+	while ((txzp_cg_read(addr + TXZP_CGSYSCR) & MCKST_MASK) != MCKST(config->mcksel)) {
+		;
+	}
+
+	txzp_cg_write(addr + TXZP_CGWUPHCR, WUPHCR_INIT_PLL(config->extfreq) | wuclk_set);
+	txzp_cg_set_bits(addr + TXZP_CGPLL0SEL, PLL0SEL_FPLL, false); /* PLL -<\> fOsc */
+	txzp_cg_set_bits(addr + TXZP_CGPLL0SEL, PLL0ON_START, false);
+	if (config->clocksel)
+		if (config->extfreq == 8)
+			fpll = CG_8Mx24_FPLL;
+		else if (config->extfreq == 12)
+			fpll = CG_12Mx16_FPLL;
+		else if (config->extfreq == 16)
+			fpll = CG_16Mx12_FPLL;
+		else if (config->extfreq == 24)
+			fpll = CG_24Mx8_FPLL;
+		else
+			fpll = 0;
+	else
+		fpll = CG_10Mx19_1875_FPLL;
+
+	txzp_cg_write(addr + TXZP_CGPLL0SEL, fpll);
+	txzp_cg_write(addr + TXZP_CGWUPHCR,
+		      WUPHCR_INIT_PLL(config->extfreq) | wuclk_set | WUON_START);
+	while ((txzp_cg_read(addr + TXZP_CGWUPHCR) & WUEF_WUP)) {
+		;
+	}
+
+	txzp_cg_write(addr + TXZP_CGWUPHCR, WUPHCR_LUPT_PLL(config->extfreq) | wuclk_set);
+	txzp_cg_set_bits(addr + TXZP_CGPLL0SEL, PLL0ON_START, true); /* PLL enabled */
+	txzp_cg_set_bits(addr + TXZP_CGOSCCR, OSCSEL_EXT, true);
+	txzp_cg_write(addr + TXZP_CGSTBYCR, config->standby);
+	txzp_cg_write(addr + TXZP_CGWUPHCR,
+		      WUPHCR_LUPT_PLL(config->extfreq) | wuclk_set | WUON_START);
+	while ((txzp_cg_read(addr + TXZP_CGWUPHCR) & WUEF_WUP)) {
+		;
+	}
+
+	txzp_cg_set_bits(addr + TXZP_CGPLL0SEL, PLL0SEL_FPLL, true); /* PLL enabled */
+	while (!(txzp_cg_read(addr + TXZP_CGPLL0SEL) & PLL0ST_FPLL)) {
+		;
+	}
+
+	return 0;
+}
+
+/**
+ * @brief Enables the clock controlled by device
+ * @param dev Clock Control device driver instance
+ * @param sys Data representing the clock
+ * @return 0 success
+ * @return else error
+ */
+static int txzp_cg_start(const struct device *dev, clock_control_subsys_t sys)
+{
+	return txzp_cg_onoff(dev, sys, true);
+}
+
+/**
+ * @brief Disables the clock controlled by device
+ * @param dev Clock Control device driver instance
+ * @param sys Data representing the clock
+ * @return 0 success
+ * @return else error
+ */
+static int txzp_cg_stop(const struct device *dev, clock_control_subsys_t sys)
+{
+	return txzp_cg_onoff(dev, sys, false);
+}
+
+/**
+ * @brief Gets the clock rate
+ * @param dev Clock Control device driver instance
+ * @param sys Data representing the clock
+ * @param rate Pointer to the variable where rate would be stored
+ * @return 0 success
+ * @return else error
+ */
+static int txzp_cg_get_rate(const struct device *dev, clock_control_subsys_t sys, uint32_t *rate)
+{
+	int ret = 0;
+	const struct txzp_cg_config *config = dev->config;
+	uint32_t addr = config->base_addr;
+	uint32_t clkid = *(uint32_t *)sys;
+	uint32_t reg = clkid >> 8;
+	uint32_t syscr;
+	uint32_t gear;
+	uint32_t prck;
+	uint32_t mckp;
+	uint32_t sysbusclk = CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC;
+
+	syscr = txzp_cg_read(addr + TXZP_CGSYSCR);
+	gear = (syscr & GEARST_MASK) >> GEARST_SHIFT;
+	if (gear > GEAR_1_16)
+		return -EINVAL;
+	prck = (syscr & PRCKST_MASK) >> PRCKST_SHIFT;
+	if (prck > PRCK_1_512)
+		return -EINVAL;
+
+	if ((reg >= TXZP_CG_SYSMENC) && (reg <= TXZP_CG_SYSMENB)){
+		mckp = (syscr & MCKPST_MASK) >> MCKPST_SHIFT;
+		if (mckp > MCKSEL_1_4)
+			mckp = MCKSEL_1_4;
+		*rate = ((sysbusclk << gear) >> prck) >> mckp;
+	}
+	else if (reg == TXZP_CG_SYSENA)
+		*rate = (sysbusclk << gear) >> prck;
+	else if (reg == TXZP_CG_FCEN)
+		*rate = sysbusclk << gear;
+	else
+		ret = -EINVAL;
+
+	return ret;
+}
+
+/**
+ * @brief Gets the APIs, the clock control driver will export
+ *
+ * API to enable the clock controlled by device
+ * API to disable the clock controlled by device
+ * API to get the clock rate
+ *
+ */
+static const struct clock_control_driver_api txzp_cg_api = {
+	.on = txzp_cg_start,
+	.off = txzp_cg_stop,
+	.get_rate = txzp_cg_get_rate,
+};
+
+/* Device Instantiation */
+#define TXZP_CG_INIT(inst)                                                                         \
+	static struct txzp_cg_config txzp_cg##inst##_config = {                                    \
+		.base_addr = DT_INST_REG_ADDR(inst),                                               \
+		.clocksel = DT_INST_PROP(inst, clocksel),                                          \
+		.extfreq = DT_INST_PROP(inst, extfreq),                                            \
+		.osc = DT_INST_PROP(inst, osc),                                                    \
+		.eosc = DT_INST_PROP(inst, eosc),                                                  \
+		.gear = DT_INST_PROP(inst, gear),                                                  \
+		.mcksel = DT_INST_PROP(inst, mcksel),                                              \
+		.prck = DT_INST_PROP(inst, prck),                                                  \
+		.standby = DT_INST_PROP(inst, standby),                                            \
+		.pll0 = DT_INST_PROP(inst, pll0),                                                  \
+	};                                                                                         \
+                                                                                                   \
+	DEVICE_DT_INST_DEFINE(inst, &txzp_cg_init, NULL, NULL, &txzp_cg##inst##_config,            \
+			      PRE_KERNEL_1, CONFIG_CLOCK_CONTROL_INIT_PRIORITY, &txzp_cg_api);
+
+DT_INST_FOREACH_STATUS_OKAY(TXZP_CG_INIT)
