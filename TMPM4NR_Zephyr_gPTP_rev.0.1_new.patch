diff --git a/drivers/gpio/gpio_txzp.c b/drivers/gpio/gpio_txzp.c
new file mode 100644
index 0000000000..7791126272
--- /dev/null
+++ b/drivers/gpio/gpio_txzp.c
@@ -0,0 +1,371 @@
+/*
+ * Copyright (C) 2023 Toshiba Electronic Devices & Storage Corporation
+ *
+ * TXZ+ GPIO driver
+ *
+ * SPDX-License-Identifier: Apache-2.0
+ */
+
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define DT_DRV_COMPAT toshiba_txzp_gpio
+
+/*------------------------------------------------------------------------------*/
+/*  Includes                                                                    */
+/*------------------------------------------------------------------------------*/
+#include <errno.h>
+#include <zephyr/device.h>
+#include <zephyr/drivers/clock_control.h>
+#include <zephyr/drivers/gpio.h>
+#include <zephyr/drivers/gpio/gpio_utils.h>
+#include <zephyr/init.h>
+#include <zephyr/kernel.h>
+#include "gpio_txzp.h"
+
+/**
+ * @brief Device configuration structure definition
+ *
+ * GPIO controller configuration for the given device
+ *
+ */
+struct gpio_txzp_cfg {
+	struct gpio_driver_config common;
+	uint32_t reg_addr;
+	int reg_size;
+	uint32_t clkid;
+	const struct device *clk_dev;
+};
+
+/**
+ * @brief GPIO device run-time data structure definition
+ */
+struct gpio_txzp_data {
+	struct gpio_driver_data common;
+};
+
+#define GPIO_DATA 0x00 /* Data Register */
+#define GPIO_CR	  0x04 /* Output Control Register */
+#define GPIO_FR1  0x08 /* Function 1 Register */
+#define GPIO_FR2  0x0c /* Function 2 Register */
+#define GPIO_FR3  0x10 /* Function 3 Register */
+#define GPIO_FR4  0x14 /* Function 4 Register */
+#define GPIO_FR5  0x18 /* Function 5 Register */
+#define GPIO_FR6  0x1c /* Function 6 Register */
+#define GPIO_FR7  0x20 /* Function 7 Register */
+#define GPIO_FR8  0x24 /* Function 8 Register */
+#define GPIO_OD	  0x28 /* Open Drain Control Register */
+#define GPIO_PUP  0x2c /* Pulup Control Register */
+#define GPIO_PDN  0x30 /* Pull Down Control Register */
+#define GPIO_IE	  0x38 /* Input Control Register */
+
+/**
+ * @brief Reads value from memory address
+ * @param config gpio configuration
+ * @param offs offset to register address
+ * @return value from register address + offset
+ */
+static inline uint32_t gpio_txzp_read(const struct gpio_txzp_cfg *config, uint32_t offs)
+{
+	return sys_read32(config->reg_addr + offs);
+}
+
+/**
+ * @brief Writes value at memory address
+ * @param config gpio configuration
+ * @param offs offset to register address
+ * @param value value to write at register + offset address
+ */
+static inline void gpio_txzp_write(const struct gpio_txzp_cfg *config, uint32_t offs,
+				   uint32_t value)
+{
+	sys_write32(value, config->reg_addr + offs);
+}
+
+/**
+ * @brief Update the bit
+ * @param config gpio configuration
+ * @param offs offset to register address
+ * @param bit bit to modify
+ * @param value value of bit
+ */
+static void gpio_txzp_modify_bit(const struct gpio_txzp_cfg *config, uint32_t offs, int bit,
+				 bool value)
+{
+	uint32_t tmp = gpio_txzp_read(config, offs);
+
+	if (value) {
+		tmp |= BIT(bit);
+	} else {
+		tmp &= ~BIT(bit);
+	}
+
+	gpio_txzp_write(config, offs, tmp);
+}
+
+/**
+ * @brief Configures general input output mode
+ * @param config gpio configuration
+ * @param gpio Pin to configure
+ * @param output true to set output mode, else input mode
+ */
+static void gpio_txzp_config_general_input_output_mode(const struct gpio_txzp_cfg *config,
+						       uint32_t gpio, bool output)
+{
+	/* follow steps in the GPIO documentation for
+	 * "Setting General Output Mode" and
+	 * "Setting General Input Mode"
+	 */
+	if (output) {
+		/* Configure positive logic in CR */
+		gpio_txzp_modify_bit(config, GPIO_CR, gpio, true);
+
+		/* Configure positive logic in IE */
+		gpio_txzp_modify_bit(config, GPIO_IE, gpio, false);
+	} else {
+		/* Configure positive logic in CR */
+		gpio_txzp_modify_bit(config, GPIO_CR, gpio, false);
+
+		/* Configure positive logic in IE */
+		gpio_txzp_modify_bit(config, GPIO_IE, gpio, true);
+	}
+}
+
+/**
+ * @brief Configures gpio pin
+ * @param dev GPIO device driver instance
+ * @param pin Pin to configure
+ * @param flags Flags for pin configuration
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_configure(const struct device *dev, gpio_pin_t pin, gpio_flags_t flags)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+
+	if ((flags & GPIO_OUTPUT) && (flags & GPIO_INPUT)) {
+		/* Pin cannot be configured as input and output */
+		return -ENOTSUP;
+	} else if (!(flags & (GPIO_INPUT | GPIO_OUTPUT))) {
+		/* Pin has to be configured as input or output */
+		return -ENOTSUP;
+	}
+
+	if (flags & GPIO_OUTPUT) {
+		if (flags & GPIO_OUTPUT_INIT_HIGH) {
+			gpio_txzp_modify_bit(config, GPIO_DATA, pin, true);
+		} else if (flags & GPIO_OUTPUT_INIT_LOW) {
+			gpio_txzp_modify_bit(config, GPIO_DATA, pin, false);
+		}
+		gpio_txzp_config_general_input_output_mode(config, pin, true);
+	} else {
+		gpio_txzp_config_general_input_output_mode(config, pin, false);
+	}
+
+	return 0;
+}
+
+/**
+ * @brief Get physical level of all input pins in a port
+ * @param dev GPIO device driver instance
+ * @param value Pointer to a variable where pin values will be stored
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_port_get_raw(const struct device *dev, gpio_port_value_t *value)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+
+	*value = gpio_txzp_read(config, GPIO_DATA);
+	return 0;
+}
+
+/**
+ * @brief Sets physical level of output pins in a port
+ * @param dev GPIO device driver instance
+ * @param mask Mask indicating which pins will be modified.
+ * @param value Value assigned to the output pins.
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_port_set_masked_raw(const struct device *dev, gpio_port_pins_t mask,
+					 gpio_port_value_t value)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+	uint32_t port_val;
+
+	port_val = gpio_txzp_read(config, GPIO_DATA);
+	port_val = (port_val & ~mask) | (value & mask);
+	gpio_txzp_write(config, GPIO_DATA, port_val);
+
+	return 0;
+}
+
+/**
+ * @brief Sets physical level of selected output pins to high.
+ * @param dev GPIO device driver instance
+ * @param pins Value indicating which pins will be modified.
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_port_set_bits_raw(const struct device *dev, gpio_port_pins_t pins)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+	uint32_t port_val;
+
+	port_val = gpio_txzp_read(config, GPIO_DATA);
+	port_val |= pins;
+	gpio_txzp_write(config, GPIO_DATA, port_val);
+
+	return 0;
+}
+
+/**
+ * @brief Clears physical level of selected output pins
+ * @param dev GPIO device driver instance
+ * @param pins Value indicating which pins will be modified.
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_port_clear_bits_raw(const struct device *dev, gpio_port_pins_t pins)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+	uint32_t port_val;
+
+	port_val = gpio_txzp_read(config, GPIO_DATA);
+	port_val &= ~pins;
+	gpio_txzp_write(config, GPIO_DATA, port_val);
+
+	return 0;
+}
+
+/**
+ * @brief Toggles physical level of selected output pins
+ * @param dev GPIO device driver instance
+ * @param pins Value indicating which pins will be modified.
+ * @return 0 success
+ * @return else error
+ */
+static int gpio_txzp_port_toggle_bits(const struct device *dev, gpio_port_pins_t pins)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+	uint32_t port_val;
+
+	port_val = gpio_txzp_read(config, GPIO_DATA);
+	port_val ^= pins;
+	gpio_txzp_write(config, GPIO_DATA, port_val);
+
+	return 0;
+}
+
+/**
+ * @brief Initializes GPIO driver
+ * @param dev GPIO device driver instance
+ * @return 0 success
+ * @return else error
+ */
+int gpio_txzp_init(const struct device *dev)
+{
+	const struct gpio_txzp_cfg *config = dev->config;
+	int ret;
+
+	if (!device_is_ready(config->clk_dev)) {
+		return -ENODEV;
+	}
+
+	ret = clock_control_on(config->clk_dev, (clock_control_subsys_t *)&config->clkid);
+	if (ret < 0) {
+		return ret;
+	}
+
+	return 0;
+}
+
+/**
+ * @brief Sets function of GPIO pins
+ * @param dev GPIO device driver instance
+ * @param pin Value indicating which pin will be modified
+ * @param function Function of the selected pin
+ * @return 0 success
+ * @return else error
+ */
+int gpio_txzp_set_function(const struct device *dev, gpio_pin_t pin, uint32_t function)
+{
+	if (8 < function) return -EINVAL;
+
+	const struct gpio_txzp_cfg *config = dev->config;
+	gpio_txzp_modify_bit(config, GPIO_FR1, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR2, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR3, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR4, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR5, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR6, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR7, pin, false);
+	gpio_txzp_modify_bit(config, GPIO_FR8, pin, false);
+	if (0 == function) return 0;
+
+	gpio_txzp_modify_bit(config, GPIO_FR1 + (function - 1) * 4, pin, true);
+	return 0;
+}
+
+/**
+ * @brief Configures the GPIO pins
+ * @param dev GPIO device driver instance
+ * @param pin Value indicating which pin will be modified
+ * @param flags Flags for pin configuration
+ * @return 0 success
+ * @return else error
+ */
+int gpio_txzp_pin_configure(const struct device *dev, gpio_pin_t pin, gpio_flags_t flags)
+{
+	if ((flags & GPIO_PULL_DOWN) && (flags & GPIO_PULL_UP)) {
+		/* Pin cannot be configured as pull-down and pull-up */
+		return -ENOTSUP;
+	}
+	const struct gpio_txzp_cfg *config = dev->config;
+	gpio_txzp_modify_bit(config, GPIO_OD, pin, (flags & GPIO_LINE_OPEN_DRAIN) ? true: false);
+	gpio_txzp_modify_bit(config, GPIO_PUP, pin, (flags & GPIO_PULL_UP) ? true: false);
+	gpio_txzp_modify_bit(config, GPIO_PDN, pin, (flags & GPIO_PULL_DOWN) ? true: false);
+	gpio_txzp_modify_bit(config, GPIO_CR, pin, (flags & GPIO_OUTPUT) ? true: false);
+	gpio_txzp_modify_bit(config, GPIO_IE, pin, (flags & GPIO_INPUT) ? true: false);
+	return 0;
+}
+
+/**
+ * @brief Gets the APIs, the gpio driver will export
+ *
+ * API to configure gpio pins
+ * API to get physical level of input pins
+ * API to set physical level of output pins in a port
+ * API to set physical level of selected output pins to high
+ * API to clear physical level of selected output pins
+ * API to toggle physical level of selected output pins
+ *
+ */
+static const struct gpio_driver_api gpio_txzp_driver_api = {
+	.pin_configure = gpio_txzp_configure,
+	.port_get_raw = gpio_txzp_port_get_raw,
+	.port_set_masked_raw = gpio_txzp_port_set_masked_raw,
+	.port_set_bits_raw = gpio_txzp_port_set_bits_raw,
+	.port_clear_bits_raw = gpio_txzp_port_clear_bits_raw,
+	.port_toggle_bits = gpio_txzp_port_toggle_bits,
+};
+
+/* Device Instantiation */
+#define GPIO_TXZP_INIT(n)                                                                          \
+	static const struct gpio_txzp_cfg gpio_txzp_cfg_##n = {                                    \
+		.common =                                                                          \
+			{                                                                          \
+				.port_pin_mask = GPIO_PORT_PIN_MASK_FROM_DT_INST(n),               \
+			},                                                                         \
+		.reg_addr = DT_INST_REG_ADDR(n),                                                   \
+		.reg_size = DT_INST_REG_SIZE(n),                                                   \
+		.clkid = DT_INST_CLOCKS_CELL(n, clkid),                                            \
+		.clk_dev = DEVICE_DT_GET(DT_INST_CLOCKS_CTLR(n)),                                  \
+	};                                                                                         \
+	static struct gpio_txzp_data gpio_txzp_data_##n;                                           \
+                                                                                                   \
+	DEVICE_DT_INST_DEFINE(n, gpio_txzp_init, NULL, &gpio_txzp_data_##n, &gpio_txzp_cfg_##n,    \
+			      PRE_KERNEL_1, CONFIG_GPIO_INIT_PRIORITY, &gpio_txzp_driver_api);
+
+DT_INST_FOREACH_STATUS_OKAY(GPIO_TXZP_INIT)
