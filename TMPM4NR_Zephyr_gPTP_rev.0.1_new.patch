+# TXZ+ Ethernet driver configuration options
+
+# Copyright (c) 2023 Toshiba Electronic Devices & Storage Corporation
+# SPDX-License-Identifier: Apache-2.0
+
+menuconfig ETH_TXZP
+	bool "M4NR HAL Ethernet driver"
+	default y
+	depends on DT_HAS_TOSHIBA_TXZP_ETHERNET_ENABLED
+	help
+	  Enable M4NR HAL based Ethernet driver.
+
+if ETH_TXZP
+
+config ETH_TXZP_RX_THREAD_STACK_SIZE
+	int "RX thread stack size"
+	default 1500
+	help
+	  RX thread stack size
+
+config ETH_TXZP_RX_THREAD_PRIO
+	int "RX thread priority"
+	default 2
+	help
+	  RX thread priority
+
+config ETH_TXZP_CARRIER_CHECK_RX_IDLE_TIMEOUT_MS
+	int "Carrier check timeout period (ms)"
+	default 500
+	range 100 30000
+	help
+	  Set the RX idle timeout period in milliseconds after which the
+	  PHY's carrier status is re-evaluated.
+
+config ETHERNET_RX_BUFFER_TIMEOUT
+	int "Received Ethernet packet allocation with buffer timeout"
+	default 100
+	help
+	  Received Ethernet packet allocation with buffer timeout, after which it is
+	  failed to obtain RX buffer.
+
+config ETH_DMA_TX_TIMEOUT_MS
+	int "Transmission check timeout (ms)"
+	default 20
+	help
+	  Set the TX timeout in milliseconds for waiting transmission to be completed
+
+config PTP_CLOCK_TXZP
+	bool "TXZP PTP clock driver support"
+	default y
+	depends on PTP_CLOCK || NET_L2_PTP
+	help
+	  Enable TXZP PTP clock support.
+
+config ETH_TXZP_PTP_CLOCK_SRC_HZ
+	int "Frequency of the clock source for the PTP timer"
+	default 50000000
+	depends on PTP_CLOCK_TXZP
+	help
+	  Set the frequency in Hz sourced to the PTP timer.
+	  If the value is set properly, the timer will be accurate.
+
+config ETH_TXZP_PTP_CLOCK_ADJ_MIN_PCT
+	int "Lower bound of clock frequency adjustment (in percent)"
+	default 90
+	depends on PTP_CLOCK_TXZP
+	help
+	  Specifies lower bound of PTP clock rate adjustment.
+
+config ETH_TXZP_PTP_CLOCK_ADJ_MAX_PCT
+	int "Upper bound of clock frequency adjustment (in percent)"
+	default 110
+	depends on PTP_CLOCK_TXZP
+	help
+	  Specifies upper bound of PTP clock rate adjustment.
+
+config ETH_TXZP_PTP_CLOCK_INIT_PRIO
+	int
+	default 85
+	depends on PTP_CLOCK_TXZP
+	help
+	  TXZP PTP Clock initialization priority level. There is
+	  a dependency from the network stack that this device
+	  initializes before network stack (NET_INIT_PRIO). 
+
+config NET_GPTP_BMCA_PRIORITY1
+	int "BMCA priority1 value"
+	default 248 if NET_GPTP_GM_CAPABLE
+	default 255
+	range 0 255
+	help
+	  The priority1 attribute of the local clock. It is used in the
+	  Best Master Clock selection Algorithm (BMCA), lower values take
+	  precedence. The default value is 255 if the device is non grand
+	  master capable, and 248 if it is GM capable.
+	  See Chapter 8.6.2.1 of IEEE 802.1AS for a more detailed description
+	  of priority1. Note that if the system	is non GM capable, then the
+	  value 255 is used always and this setting is ignored.
+
+config NET_GPTP_BMCA_PRIORITY2
+	int "BMCA priority2 value"
+	default 248
+	range 0 255
+	help
+	  The priority2 attribute of the local clock. It is used in the BMCA
+	  (Best Master Clock selection Algorithm), lower values take
+	  precedence. The default value is 248.
+	  See Chapter 8.6.2.5 of IEEE 802.1AS for a more detailed description
+	  of priority2.
+
+
+endif # ETH_TXZP
diff --git a/drivers/ethernet/eth_txzp.c b/drivers/ethernet/eth_txzp.c
new file mode 100644
index 0000000000..dfda618c28
--- /dev/null
+++ b/drivers/ethernet/eth_txzp.c
@@ -0,0 +1,1020 @@
+/*
+ * Copyright (C) 2023 Toshiba Electronic Devices & Storage Corporation
+ *
+ * TXZ+ Ethernet driver
+ *
+ * SPDX-License-Identifier: Apache-2.0
+ */
+
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define DT_DRV_COMPAT toshiba_txzp_ethernet
+#define LOG_MODULE_NAME eth_txzp
+#define LOG_LEVEL CONFIG_ETHERNET_LOG_LEVEL
+
+/*------------------------------------------------------------------------------*/
+/*  Includes                                                                    */
+
+#include <zephyr/kernel.h>
+#include <zephyr/device.h>
+#include <zephyr/sys/__assert.h>
+#include <zephyr/sys/util.h>
+#include <errno.h>
+#include <stdbool.h>
+#include <zephyr/net/net_pkt.h>
+#include <zephyr/net/net_if.h>
+#include <zephyr/net/ethernet.h>
+#include <ethernet/eth_stats.h>
+#include <soc.h>
+#include <zephyr/sys/printk.h>
+#include <zephyr/drivers/clock_control.h>
+#include <zephyr/drivers/pinctrl.h>
+#include <zephyr/irq.h>
+#include <zephyr/logging/log.h>
+#include "TMPM4Nx.h"
+#include "txz_ethm.h"
+#include "eth.h"
+#include "eth_txzp_priv.h"
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	#include <zephyr/drivers/ptp_clock.h>
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+LOG_MODULE_REGISTER(LOG_MODULE_NAME);
+
+/* Receive buffer	*/
+static uint8_t rx_buffer[ETHM_RX_BUF_SIZE];
+
+/* EMAC Handle */
+static ethm_t *heth_handle;
+
+/**
+ * @brief Read ethernet Physical Register
+ * @param heth EMAC Handle
+ * @param PHYAddr physical layer address
+ * @param PHYReg physical register address
+ * @param RegVal pointer to store read data
+ * @return TXZ_SUCCESS Success
+ * @return TXZ_ERROR Failure
+ */
+static TXZ_Result eth_txzp_read_phy_register(ethm_t *heth,
+						uint8_t PHYAddr,
+						uint8_t PHYReg,
+						uint16_t *RegVal)
+{
+	/*	Read a PHY Register	*/
+	return ethm_phy_read(heth->p_instance, PHYAddr, PHYReg, RegVal);
+}
+
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+/**
+ * @brief Checks if Packet is PTP packet
+ * @param iface Network interface structure
+ * @param pkt Network pkt to be transmitted
+ * @return true Packet is a ptp packet
+ * @return false Packet is Not a ptp packet	 
+ */
+static bool eth_is_ptp_pkt(struct net_if *iface, struct net_pkt *pkt)
+{
+	__ASSERT_NO_MSG(pkt != NULL);
+	__ASSERT_NO_MSG(iface != NULL);
+	
+	if (ntohs(NET_ETH_HDR(pkt)->type) != NET_ETH_PTYPE_PTP) {
+		return false;
+	}
+
+	net_pkt_set_priority(pkt, NET_PRIORITY_CA);
+
+	return true;
+}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+
+/**
+ * @brief Transmit ethernet packet.
+ * @param dev Ethernet device driver instance
+ * @param pkt Network pkt to be transmitted
+ * @return zero successful transmission
+ * @return else error
+ */
+static int eth_txzp_tx(const struct device *dev, struct net_pkt *pkt)
+{
+	struct eth_txzp_dev_data *dev_data = dev->data;
+	ethm_t *heth;
+	uint8_t *dma_buffer;
+	int32_t res=0;
+	size_t total_len;
+	__IO TSB_ETHM_DMADesc_TypeDef *dma_tx_desc;
+	TXZ_Result hal_ret = TXZ_SUCCESS;
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	bool timestamped_frame;
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+
+	__ASSERT_NO_MSG(pkt != NULL);
+	__ASSERT_NO_MSG(pkt->frags != NULL);
+	__ASSERT_NO_MSG(dev != NULL);
+	__ASSERT_NO_MSG(dev_data != NULL);
+
+	heth = &dev_data->heth;
+	dma_tx_desc = heth->p_tx_desc;
+	k_mutex_lock(&dev_data->tx_mutex, K_FOREVER);
+
+	total_len = net_pkt_get_len(pkt);
+	if (total_len > ETHM_TX_BUF_SIZE) {
+		LOG_ERR("PKT too big");
+		res = -EIO;
+		goto error;
+	}
+
+	/* Wait till DMA release the descriptor */
+	while (IS_ETH_DMATXDESC_OWN(dma_tx_desc) != (uint32_t)RESET) {
+		k_yield();
+	}
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	timestamped_frame = eth_is_ptp_pkt(net_pkt_iface(pkt), pkt);
+	if (timestamped_frame) {
+		/* Enable transmit timestamp */
+		dma_tx_desc->des0 |= TX_DMA_DESC0_TTSE;
+	}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+	dma_buffer = (uint8_t *)(heth->p_tx_desc->des2_Buffer_1_Addr) ;
+
+	if (net_pkt_read(pkt, dma_buffer, total_len)) {
+		res = -ENOBUFS;
+		goto error;
+	}
+
+	/* Reset TX complete interrupt semaphore before TX request*/
+	k_sem_reset(&dev_data->eth_tx_sem);
+
+	/* Transmit ethernet frame */
+	hal_ret = ethm_transmit(heth,dma_buffer,total_len);
+	if (hal_ret != TXZ_SUCCESS) {
+		LOG_ERR("ethernet transmit: failed!");
+		res = -EIO;
+		goto error;
+	}
+
+	/* Wait for end of TX buffer transmission */
+	if (k_sem_take(&dev_data->eth_tx_sem,
+			K_MSEC(CONFIG_ETH_DMA_TX_TIMEOUT_MS)) != 0) {
+
+		LOG_ERR("ethernet transmit failed or ethernet semaphore take timeout occurred");
+		res = -EIO;
+		goto error;
+	}
+
+	/* 
+	 * Issue a Transmit Poll Demand to resume transmission.
+	 * When Transmit Underflow flag is set and clear it.
+	 */
+	if (TSB_EMA_STATUS_UNF) {
+		/* Clear UNF ETHERNET DMA flag */
+		TSB_EMA_STATUS_UNF = 1;
+		/* Resume DMA transmission*/
+		heth->p_instance->TRANSMIT_POLL_DEMAND = 0;
+		res = -EIO;
+		goto error;
+	}
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	if (timestamped_frame) {
+		/* Retrieve transmission timestamp from last DMA TX descriptor */
+		__IO TSB_ETHM_DMADesc_TypeDef *last_dma_tx_desc = dma_tx_desc;
+
+		while (!(last_dma_tx_desc->des0 & TX_DMA_DESC0_LS) &&
+				last_dma_tx_desc->des3_Next_Desc_Addr) {
+			last_dma_tx_desc = (TSB_ETHM_DMADesc_TypeDef *)last_dma_tx_desc->des3_Next_Desc_Addr;
+		}
+	
+		while (IS_ETH_DMATXDESC_OWN(last_dma_tx_desc) != (uint32_t)RESET) {
+			/* Wait for transmission */
+			k_yield();
+		}
+		if (last_dma_tx_desc->des0 & TX_DMA_DESC0_LS &&
+				last_dma_tx_desc->des0 & TX_DMA_DESC0_TTSS) {
+			pkt->timestamp.second = last_dma_tx_desc->Time_Stamp_High;
+			pkt->timestamp.nanosecond = last_dma_tx_desc->Time_Stamp_Low;
+							
+		} else {
+			/* Invalid value */
+			pkt->timestamp.second = UINT64_MAX;
+			pkt->timestamp.nanosecond = UINT32_MAX;
+		}
+		net_if_add_tx_timestamp(pkt);
+	}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+	res = 0;
+error:
+	k_mutex_unlock(&dev_data->tx_mutex);
+
+	return res;
+}
+
+/**
+ * @brief Gets the network interface
+ * @param ctx Device runtime data
+ * @return net_if network interface
+ */
+static struct net_if *eth_txzp_get_iface(struct eth_txzp_dev_data *ctx)
+{
+	return ctx->iface;
+}
+
+/**
+ * @brief Receives ethernet packet
+ * @param dev Ethernet device driver instance
+ * @return net_pkt network packet
+ */
+static struct net_pkt *eth_txzp_rx(const struct device *dev)
+{
+	struct eth_txzp_dev_data *dev_data;
+	ethm_t *heth;
+	struct net_pkt *pkt;
+	uint32_t rx_len;
+	uint8_t *dma_buffer = &rx_buffer[0];
+	TXZ_Result hal_ret = TXZ_SUCCESS;
+	__IO TSB_ETHM_DMADesc_TypeDef *last_dma_rx_desc;
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+		struct net_ptp_time timestamp;
+		/* Default to invalid value. */
+		timestamp.second = UINT64_MAX;
+		timestamp.nanosecond = UINT32_MAX;
+#endif /* CONFIG_PTP_CLOCK_TXZP */ 
+
+	__ASSERT_NO_MSG(dev != NULL);
+	dev_data = dev->data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	heth = &dev_data->heth;
+
+	last_dma_rx_desc = heth->p_rx_desc;
+	hal_ret = ethm_receive(heth, dma_buffer, &rx_len);
+	if (hal_ret != TXZ_SUCCESS) {
+		/* no frame available */
+		return NULL;
+	}
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	while (!(last_dma_rx_desc->des0 & RX_DMA_DESC0_LS) && 
+			last_dma_rx_desc->des3_Next_Desc_Addr) {	
+		last_dma_rx_desc = (TSB_ETHM_DMADesc_TypeDef *)last_dma_rx_desc->des3_Next_Desc_Addr;
+	}
+	
+	if (last_dma_rx_desc->des0 & RX_DMA_DESC0_LS &&
+			last_dma_rx_desc->des0 & RX_DMA_DESC0_TTSS) {
+		timestamp.second = last_dma_rx_desc->Time_Stamp_High;
+		timestamp.nanosecond = last_dma_rx_desc->Time_Stamp_Low;
+	}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+	pkt = net_pkt_rx_alloc_with_buffer(eth_txzp_get_iface(dev_data),
+					   rx_len, AF_UNSPEC, 0, K_MSEC(CONFIG_ETHERNET_RX_BUFFER_TIMEOUT));
+	if (!pkt) {
+		LOG_ERR("Failed to obtain RX buffer");
+		goto out;
+	}
+
+	if (net_pkt_write(pkt, dma_buffer, rx_len)) {
+		LOG_ERR("Failed to append RX buffer to context buffer");
+		net_pkt_unref(pkt);
+		pkt = NULL;
+		goto out;
+	}
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	if (eth_is_ptp_pkt(eth_txzp_get_iface(dev_data), pkt)) {
+		pkt->timestamp.second = timestamp.second;
+		pkt->timestamp.nanosecond = timestamp.nanosecond;
+	} else {
+		/* Invalid value */
+		pkt->timestamp.second = UINT64_MAX;
+		pkt->timestamp.nanosecond = UINT32_MAX;
+	}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+out:
+	if (!pkt) {
+		eth_stats_update_errors_rx(eth_txzp_get_iface(dev_data));
+	}
+	return pkt;
+}
+
+/**
+ * @brief Receive Thread
+ * @param arg1 First entry point parameter
+ * @param unused1 Second entry point parameter
+ * @param unused2 Third entry point parameter
+ */
+static void eth_txzp_rx_thread(void *arg1, void *unused1, void *unused2)
+{
+	const struct device *dev;
+	struct eth_txzp_dev_data *dev_data;
+	struct net_pkt *pkt;
+	int32_t res;
+	uint16_t status;
+	TXZ_Result hal_ret = TXZ_SUCCESS;
+
+	__ASSERT_NO_MSG(arg1 != NULL);
+	ARG_UNUSED(unused1);
+	ARG_UNUSED(unused2);
+	dev = (const struct device *)arg1;
+	dev_data = dev->data;
+
+	__ASSERT_NO_MSG(dev_data != NULL);
+
+	while (1) {
+		res = k_sem_take(&dev_data->eth_rx_sem,
+			K_MSEC(CONFIG_ETH_TXZP_CARRIER_CHECK_RX_IDLE_TIMEOUT_MS));
+
+		if (res == 0) {
+			/* semaphore taken, update link status and receive packets */
+			if (dev_data->link_up != true) {
+				dev_data->link_up = true;
+				net_eth_carrier_on(eth_txzp_get_iface(dev_data));
+			}
+			while ((pkt = eth_txzp_rx(dev)) != NULL) {
+				res = net_recv_data(net_pkt_iface(pkt), pkt);
+				if (res < 0) {
+					eth_stats_update_errors_rx(
+							net_pkt_iface(pkt));
+					LOG_ERR("Failed to enqueue frame "
+						"into RX queue: %d", res);
+					net_pkt_unref(pkt);
+				}
+			}
+
+		} else if (res == -EAGAIN) {
+			// semaphore timeout period expired, check link status
+			hal_ret = eth_txzp_read_phy_register(&dev_data->heth,
+				    TMPM4NR_PHY_ADDR, TMPM4NR_PHY_BSR, &status);
+			if (hal_ret == TXZ_SUCCESS) {
+				if ((status & TMPM4NR_PHY_BSR_LINK) == TMPM4NR_PHY_BSR_LINK) {
+					if (dev_data->link_up != true) {
+						dev_data->link_up = true;
+						net_eth_carrier_on(
+							eth_txzp_get_iface(dev_data));
+					}
+				} else {
+					if (dev_data->link_up != false) {
+						dev_data->link_up = false;
+						net_eth_carrier_off(
+							eth_txzp_get_iface(dev_data));
+					}
+				}
+			}
+			else {
+				LOG_ERR("Timeout occurred to read PHY register");
+			}
+		}
+	}
+}
+
+/**
+ * @brief ISR to process transmit and receive interrupts
+ *
+ */
+static void eth_txzp_isr()
+{
+	INTEMA_IRQHandler();
+}
+
+/**
+ * @brief Callback function to notify data reception
+ * @param result Notify the success or failure of rx callback
+ */
+void eth_txzp_rx_callback(TXZ_Result result)
+{
+	if (result == TXZ_ERROR) {
+		return;
+	}
+	__ASSERT_NO_MSG(heth_handle != NULL);
+
+	struct eth_txzp_dev_data *dev_data =
+		CONTAINER_OF(heth_handle, struct eth_txzp_dev_data, heth);
+
+	__ASSERT_NO_MSG(dev_data != NULL);
+
+	/* Release semaphore */
+	k_sem_give(&dev_data->eth_rx_sem);
+}
+
+/**
+ * @brief Callback function to notify data transmission
+ * @param result Notify the success or failure of tx callback
+ */
+void eth_txzp_tx_callback(TXZ_Result result)
+{
+	if (result == TXZ_ERROR) {
+		return;
+	}
+	__ASSERT_NO_MSG(heth_handle != NULL);
+
+	struct eth_txzp_dev_data *dev_data =
+		CONTAINER_OF(heth_handle, struct eth_txzp_dev_data, heth);
+
+	__ASSERT_NO_MSG(dev_data != NULL);
+
+	/* Release semaphore */
+	k_sem_give(&dev_data->eth_tx_sem);
+}
+
+/**
+ * @brief Initialize Ethernet controller
+ *
+ * Register callback function to notify data reception and data transmission
+ * start interruption poll thread
+ *
+ * @param dev Ethernet device driver instance
+ * @return zero successful initialization
+ * @return else error
+ */
+static int eth_txzp_initialize(const struct device *dev)
+{
+	struct eth_txzp_dev_data *dev_data;
+	const struct eth_txzp_dev_config *cfg;
+	ethm_t *heth;
+	TXZ_Result hal_ret = TXZ_SUCCESS;
+	int ret = 0;
+
+	__ASSERT_NO_MSG(dev != NULL);
+
+	dev_data = dev->data;
+	cfg = dev->config;
+
+	__ASSERT_NO_MSG(dev_data != NULL);
+	__ASSERT_NO_MSG(cfg != NULL);
+
+	if (!device_is_ready(dev_data->clock)) {
+		LOG_ERR("clock control device not ready");
+		return -ENODEV;
+	}
+
+	/* enable clock */
+	ret = clock_control_on(dev_data->clock, (clock_control_subsys_t *)&cfg->clkid);
+	if (ret < 0) {
+		LOG_ERR("Failed call clock_control_on: return [%d]", ret);
+		return -EIO;
+	}
+
+	/* configure pinmux */
+	ret = pinctrl_apply_state(cfg->pincfg, PINCTRL_STATE_DEFAULT);
+	if (ret < 0) {
+		LOG_ERR("Could not configure ethernet pins");
+		return ret;
+	}
+
+	heth = &dev_data->heth;
+
+	/* Initializing heth_handle for callback purpose */
+	heth_handle = heth;
+
+	/* Register callback function to notify data transmission  */
+	if (ethm_register_tx_callback(heth,eth_txzp_tx_callback) != TXZ_SUCCESS) {
+		LOG_ERR("Tx Callback not Registered");
+    }
+
+	/* Register callback function to notify data reception  */
+	if (ethm_register_rx_callback(heth,eth_txzp_rx_callback) != TXZ_SUCCESS) {
+		LOG_ERR("Rx Callback not Registered");
+    }
+
+	dev_data->link_up = false;
+
+	/* Initialize semaphores */
+	k_mutex_init(&dev_data->tx_mutex);
+	k_sem_init(&dev_data->eth_rx_sem,0, K_SEM_MAX_LIMIT);
+	k_sem_init(&dev_data->eth_tx_sem,0, K_SEM_MAX_LIMIT);
+
+	/* Start interruption-poll thread */
+	k_thread_create(&dev_data->rx_thread, dev_data->rx_thread_stack,
+			K_KERNEL_STACK_SIZEOF(dev_data->rx_thread_stack),
+			eth_txzp_rx_thread, (void *) dev, NULL, NULL,
+			K_PRIO_COOP(CONFIG_ETH_TXZP_RX_THREAD_PRIO),
+			0, K_NO_WAIT);
+
+	k_thread_name_set(&dev_data->rx_thread, "txzp_eth");
+
+	/* Initialize and Enable DMA and EMAC	*/
+	hal_ret = ethm_init(heth);
+#if defined(CONFIG_PTP_CLOCK_TXZP)		 
+	/* Enable Frame PTP 
+	 */
+	TSB_EMA_TIMESTAMP_CONTROL_TSIPENA = 1;
+	TSB_EMA_TIMESTAMP_CONTROL_TSVER2ENA = 1;
+	/* Enable timestamping of RX packets. We enable all packets to be
+	 * timestamped to cover both IEEE 1588 and gPTP.
+	 */			
+	TSB_EMA_TIMESTAMP_CONTROL_TSENALL = 1;
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+	if (hal_ret != TXZ_SUCCESS) {
+		LOG_ERR("DMA or EMAC not initialized");
+		return -EINVAL;
+	}
+
+	/* Default MAC Address	*/
+
+	dev_data->mac_addr[0] = MAC_ADDR_0;
+	dev_data->mac_addr[1] = MAC_ADDR_1;
+	dev_data->mac_addr[2] = MAC_ADDR_2;
+	dev_data->mac_addr[3] = MAC_ADDR_3;
+	dev_data->mac_addr[4] = MAC_ADDR_4;
+	dev_data->mac_addr[5] = MAC_ADDR_5;
+
+	LOG_DBG("MAC %02x:%02x:%02x:%02x:%02x:%02x",
+		dev_data->mac_addr[0], dev_data->mac_addr[1],
+		dev_data->mac_addr[2], dev_data->mac_addr[3],
+		dev_data->mac_addr[4], dev_data->mac_addr[5]);
+
+	return 0;
+}
+
+/**
+ * @brief Initializes ethernet interface
+ * @param iface Network interface structure
+ */
+static void eth_txzp_iface_init(struct net_if *iface)
+{
+	const struct device *dev;
+	struct eth_txzp_dev_data *dev_data;
+	bool is_first_init = false;
+
+	__ASSERT_NO_MSG(iface != NULL);
+
+	dev = net_if_get_device(iface);
+	__ASSERT_NO_MSG(dev != NULL);
+
+	dev_data = dev->data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+
+	if (dev_data->iface == NULL) {
+		dev_data->iface = iface;
+		is_first_init = true;
+	}
+
+	/* Register Ethernet MAC Address with the upper layer */
+	net_if_set_link_addr(iface, dev_data->mac_addr,
+			     sizeof(dev_data->mac_addr),
+			     NET_LINK_ETHERNET);
+
+	ethernet_init(iface);
+
+	net_if_carrier_off(iface);
+
+	if (is_first_init) {
+		const struct eth_txzp_dev_config *cfg = dev->config;
+		/* Now that the iface is setup, we are safe to enable IRQs. */
+		__ASSERT_NO_MSG(cfg->config_func != NULL);
+		cfg->config_func(dev);
+	}
+}
+
+/**
+ * @brief Sets the ethernet configuration
+ * @param dev Ethernet device driver instance
+ * @param type Ethernet configuration type
+ * @param config Ethernet configuration
+ * @return zero successful configuration
+ * @return else error
+ */
+static int eth_txzp_set_config(const struct device *dev,
+				    enum ethernet_config_type type,
+				    const struct ethernet_config *config)
+{
+	int ret = -ENOTSUP;
+	struct eth_txzp_dev_data *dev_data;
+	ethm_t *heth;
+
+	dev_data = dev->data;
+	heth = &dev_data->heth;
+
+	switch (type) {
+	case ETHERNET_CONFIG_TYPE_MAC_ADDRESS:
+		memcpy(dev_data->mac_addr, config->mac_address.addr, MAC_ADDR_SIZE);
+		ethm_set_mac_address(heth,&(dev_data->mac_addr[0]));
+		net_if_set_link_addr(dev_data->iface, dev_data->mac_addr,
+				     sizeof(dev_data->mac_addr),
+				     NET_LINK_ETHERNET);
+		ret = 0;
+		break;
+	case ETHERNET_CONFIG_TYPE_PROMISC_MODE:
+#if defined(CONFIG_NET_PROMISCUOUS_MODE)
+		if (config->promisc_mode) {
+			TSB_EMA_TMAC_FRAME_FILTER_PM = 1;
+		} else {
+			TSB_EMA_TMAC_FRAME_FILTER_PM = 0;
+		}
+		ret = 0;
+#endif /* CONFIG_NET_PROMISCUOUS_MODE */
+		break;
+	default:
+		break;
+	}
+
+	return ret;
+}
+
+/**
+ * @brief Gets the hardware capabilities of ethernet
+ * @param dev Ethernet device driver instance
+ * @return Ethernet hardware capabilities
+ */
+static enum ethernet_hw_caps eth_txzp_get_capabilities(const struct device *dev)
+{
+	ARG_UNUSED(dev);
+
+	return ETHERNET_LINK_10BASE_T | ETHERNET_LINK_100BASE_T
+#if defined(CONFIG_NET_VLAN)
+		| ETHERNET_HW_VLAN
+#endif
+#if defined(CONFIG_NET_PROMISCUOUS_MODE)
+		| ETHERNET_PROMISC_MODE
+#endif
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+		| ETHERNET_PTP
+#endif
+		;
+}
+
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+/**
+ * @brief Gets the ptp clock from device driver instance
+ * @param dev Ethernet device driver instance
+ * @return ptp clock
+ */
+static const struct device *eth_txzp_get_ptp_clock(const struct device *dev)
+{
+	__ASSERT_NO_MSG(dev != NULL);
+	struct eth_txzp_dev_data *dev_data = dev->data;
+	return dev_data->ptp_clock;
+}
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+/**
+ * @brief Gets the APIs, the ethernet driver will export
+ *
+ * API to initialize ethernet interface
+ * API to get hardware capabilities
+ * API to set ethernet configurations
+ * API to send ethernet data
+ *
+ */
+static const struct ethernet_api eth_txzp_driver_api = {
+	.iface_api.init = eth_txzp_iface_init,
+	.get_capabilities = eth_txzp_get_capabilities,
+	.set_config = eth_txzp_set_config,
+	.send = eth_txzp_tx,
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+	.get_ptp_clock = eth_txzp_get_ptp_clock,
+#endif /* CONFIG_PTP_CLOCK_TXZP */
+};
+
+
+/* Device Instantiation */
+#define ETH_TXZP_INIT(n)							\
+	PINCTRL_DT_INST_DEFINE(n);						\
+										\
+	static void eth_txzp_config_func_##n(const struct device *dev); 	\
+										\
+	static const struct eth_txzp_dev_config eth_txzp_config_##n = {		\
+		.config_func = eth_txzp_config_func_##n,			\
+		.pincfg = PINCTRL_DT_INST_DEV_CONFIG_GET(n),			\
+		.clkid = DT_INST_CLOCKS_CELL(n, clkid),	};			\
+										\
+	static struct eth_txzp_dev_data eth_txzp_data_##n = {			\
+		.heth = {							\
+			.p_instance = (TSB_EM_TypeDef *)DT_INST_REG_ADDR(n), 	\
+		},								\
+		.clock = DEVICE_DT_GET(DT_INST_CLOCKS_CTLR(n)),			\
+		};								\
+										\
+	ETH_NET_DEVICE_DT_INST_DEFINE(n,					\
+			eth_txzp_initialize, NULL,				\
+			&eth_txzp_data_##n,					\
+			&eth_txzp_config_##n,					\
+			CONFIG_ETH_INIT_PRIORITY,				\
+			&eth_txzp_driver_api,					\
+			ETH_TXZP_MTU);						\
+										\
+	static void eth_txzp_config_func_##n(const struct device *dev)		\
+	{									\
+		ARG_UNUSED(dev);                                        	\
+										\
+		IRQ_CONNECT(DT_INST_IRQN(n),					\
+			DT_INST_IRQ(n, priority),				\
+			eth_txzp_isr,						\
+			DEVICE_DT_INST_GET(n), 0);				\
+										\
+		irq_enable(DT_INST_IRQN(n));					\
+	}
+
+
+DT_INST_FOREACH_STATUS_OKAY(ETH_TXZP_INIT)
+#if defined(CONFIG_PTP_CLOCK_TXZP)
+
+struct ptp_context {
+	struct eth_txzp_dev_data *dev_data;
+};
+
+static struct ptp_context ptp_txzp_0_context;
+
+/**
+ * @brief Set System Time
+ *
+ * Update system time registers with net ptp time structure
+ *
+ * @param dev Ethernet device driver instance
+ * @param tm  Timestamp according to the Precision Time Protocol standard
+ * @return zero ptp clock set successfully
+ * @return else error
+ */
+static int ptp_clock_txzp_set(const struct device *dev,
+			      struct net_ptp_time *tm)
+{
+	struct ptp_context *ptp_context = dev->data;
+	__ASSERT_NO_MSG(ptp_context != NULL);
+	
+	struct eth_txzp_dev_data *dev_data = ptp_context->dev_data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	
+	ethm_t *heth = &dev_data->heth;
+	unsigned int key;
+	
+	__ASSERT_NO_MSG(dev != NULL);
+	__ASSERT_NO_MSG(tm != NULL);
+	
+
+	key = irq_lock();
+
+	heth->p_instance->SYSTEM_TIME_SECONDS_UPDATE = tm->second;
+	heth->p_instance->SYSTEM_TIME_NANOSECONDS_UPDATE = tm->nanosecond;
+
+	irq_unlock(key);
+		
+	TSB_EMA_TIMESTAMP_CONTROL_TSINIT = 1;
+	while (TSB_EMA_TIMESTAMP_CONTROL_TSINIT) {
+		/* spin lock */
+	}
+	return 0;
+}
+
+/**
+ * @brief Get System Time
+ *
+ * Update net ptp time registers with system time registers
+ *
+ * @param dev Ethernet device driver instance
+ * @param tm  Timestamp according to the Precision Time Protocol standard
+ * @return zero successful get ptp clock
+ * @return else error
+ */
+static int ptp_clock_txzp_get(const struct device *dev,
+			      struct net_ptp_time *tm)
+{
+	struct ptp_context *ptp_context = dev->data;
+	__ASSERT_NO_MSG(ptp_context != NULL);
+	
+	struct eth_txzp_dev_data *dev_data = ptp_context->dev_data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	
+	ethm_t *heth = &dev_data->heth;
+	unsigned int key;
+	uint32_t second_2;
+	
+	__ASSERT_NO_MSG(dev != NULL);
+	__ASSERT_NO_MSG(tm != NULL);
+	
+	key = irq_lock();
+
+	tm->second = heth->p_instance->SYSTEM_TIME_SECONDS;
+	tm->nanosecond = heth->p_instance->SYSTEM_TIME_NANOSECONDS;
+	second_2 = heth->p_instance->SYSTEM_TIME_SECONDS;
+	irq_unlock(key);
+
+	if (tm->second != second_2 && tm->nanosecond < NSEC_PER_SEC / 2) {
+		/* We will use second_2 as a new second value.
+		 * Second rollover has happened during first measurement: second register
+		 * was read before second boundary and nanosecond register was read after.
+		 */
+		tm->second = second_2;
+	}
+
+	return 0;
+}
+
+/**
+ * @brief Clock Adjust
+ *
+ * Update the clock
+ *
+ * @param dev Ethernet device driver instance
+ * @param increment Update nanoseconds
+ * @return zero clock adjusted successfully
+ * @return else error
+ */
+static int ptp_clock_txzp_adjust(const struct device *dev, int increment)
+{
+	struct ptp_context *ptp_context = dev->data;
+	__ASSERT_NO_MSG(ptp_context != NULL);
+	
+	struct eth_txzp_dev_data *dev_data = ptp_context->dev_data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	
+	ethm_t *heth = &dev_data->heth;
+	int key, ret;
+
+	__ASSERT_NO_MSG(dev != NULL);
+	
+	if ((increment <= (int32_t)(-NSEC_PER_SEC)) ||
+			(increment >= (int32_t)NSEC_PER_SEC)) {
+		ret = -EINVAL;
+	} else {
+		key = irq_lock();
+		
+		heth->p_instance->SYSTEM_TIME_SECONDS_UPDATE = 0;
+		if (increment >= 0) {
+			heth->p_instance->SYSTEM_TIME_NANOSECONDS_UPDATE = increment;
+		} else {
+			TSB_EMA_SYSTEM_TIME_NANOSECONDS_UPDATE_ADDSUB |= (-increment);
+		}
+		TSB_EMA_TIMESTAMP_CONTROL_TSUPDT = 1;
+		while (TSB_EMA_TIMESTAMP_CONTROL_TSUPDT) {
+			/* spin lock */
+		}
+
+		ret = 0;
+		irq_unlock(key);
+	}
+
+	return ret;
+}
+
+/**
+ * @brief Clock Rate Adjust
+ *
+ * Adjust the clock rate.
+ *
+ * @param dev Ethernet device driver instance
+ * @param ratio Clock ratio
+ * @return zero clock rate adjusted successfully
+ * @return else error
+ */
+
+static int ptp_clock_txzp_rate_adjust(const struct device *dev, double ratio)
+{
+	struct ptp_context *ptp_context = dev->data;
+	__ASSERT_NO_MSG(ptp_context != NULL);
+	
+	struct eth_txzp_dev_data *dev_data = ptp_context->dev_data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	
+	ethm_t *heth = &dev_data->heth;
+	int key, ret;
+	uint32_t addend_val;
+	
+	__ASSERT_NO_MSG(dev != NULL);
+	
+	/* No change needed */
+	if (ratio == 1.0f) {
+		return 0;
+	}
+
+	key = irq_lock();
+
+	ratio *= dev_data->clk_ratio_adj;
+
+	/* Limit possible ratio */
+	if (ratio * 100 < CONFIG_ETH_TXZP_PTP_CLOCK_ADJ_MIN_PCT ||
+			ratio * 100 > CONFIG_ETH_TXZP_PTP_CLOCK_ADJ_MAX_PCT) {
+		ret = -EINVAL;
+		goto error;
+	}
+
+	/* Save new ratio */
+	dev_data->clk_ratio_adj = ratio;
+
+	/* Update addend register */
+	addend_val = UINT32_MAX * dev_data->clk_ratio * ratio;
+
+	heth->p_instance->TIMESTAMP_ADDEND = addend_val;
+	TSB_EMA_TIMESTAMP_CONTROL_TSADDREG = 1;
+	while (TSB_EMA_TIMESTAMP_CONTROL_TSADDREG) {
+		/* spin lock */
+	}
+	ret = 0;
+
+error:
+	irq_unlock(key);
+
+	return ret;
+}
+
+static const struct ptp_clock_driver_api api = {
+	.set = ptp_clock_txzp_set,
+	.get = ptp_clock_txzp_get,
+	.adjust = ptp_clock_txzp_adjust,
+	.rate_adjust = ptp_clock_txzp_rate_adjust,
+};
+
+/**
+ * @brief Initialize PTP
+ *
+ * Initialize and enable timestamping
+ *
+ * @param port Ethernet device driver instance
+ * @return zero successful initialization
+ * @return else error
+ */
+
+static int ptp_txzp_init(const struct device *port)
+{
+	const struct device *const dev = DEVICE_DT_GET(DT_NODELABEL(mac));
+	__ASSERT_NO_MSG(dev != NULL);
+	
+	struct eth_txzp_dev_data *dev_data = dev->data;
+	const struct eth_txzp_dev_config *cfg = dev->config;
+	__ASSERT_NO_MSG(port != NULL);
+	
+	struct ptp_context *ptp_context = port->data;
+	__ASSERT_NO_MSG(dev_data != NULL);
+	
+	ethm_t *heth = &dev_data->heth;
+	int ret;
+	uint32_t ptp_hclk_rate;
+	uint32_t ss_incr_ns;
+	uint32_t addend_val;
+	
+	__ASSERT_NO_MSG(cfg != NULL);
+	__ASSERT_NO_MSG(ptp_context != NULL);
+
+	dev_data->ptp_clock = port;
+	ptp_context->dev_data = dev_data;
+
+	/* Mask the Timestamp Trigger interrupt */
+	TSB_EMA_INTERRUPT_MASK_TSIM = 1;
+
+
+	/* Enable timestamping */
+	TSB_EMA_TIMESTAMP_CONTROL_TSENA = 1;
+
+	
+	/* Query ethernet clock rate */
+	ret = clock_control_get_rate(dev_data->clock, (clock_control_subsys_t *)&cfg->clkid, &ptp_hclk_rate);
+	if (ret) {
+		LOG_ERR("Failed to query ethernet clock");
+		return -EIO;
+	}
+
+	/* Program the subsecond increment register based on the PTP clock freq */
+	if (NSEC_PER_SEC % CONFIG_ETH_TXZP_PTP_CLOCK_SRC_HZ != 0) {
+		LOG_ERR("PTP clock period must be an integer nanosecond value");
+		return -EINVAL;
+	}
+	ss_incr_ns = NSEC_PER_SEC / CONFIG_ETH_TXZP_PTP_CLOCK_SRC_HZ;
+	if (ss_incr_ns > UINT8_MAX) {
+		LOG_ERR("PTP clock period is more than %d nanoseconds", UINT8_MAX);
+		return -EINVAL;
+	}
+
+	heth->p_instance->SUB_SECOND_INCREMENT = ss_incr_ns;
+
+	/* Program timestamp addend register */
+	dev_data->clk_ratio =
+		((double)CONFIG_ETH_TXZP_PTP_CLOCK_SRC_HZ) / ((double)ptp_hclk_rate);
+	/*
+	 * clk_ratio_adj compensates for inaccuracies caused by manufacturing tolerances
+	 * and environmental effects (e.g. temperature). HCLK is defined by a physical 
+	 * oscillator and hence it might drift. 
+	 * clk_ratio is a ratio between desired PTP clock frequency and ptp HCLK rate.
+	 * It starts off as 1.0 and gets adjusted by calling ptp_clock_txzp_rate_adjust().
+	 */
+	dev_data->clk_ratio_adj = 1.0f;
+	addend_val =
+		UINT32_MAX * dev_data->clk_ratio * dev_data->clk_ratio_adj;
+
+	heth->p_instance->TIMESTAMP_ADDEND = addend_val;
+	TSB_EMA_TIMESTAMP_CONTROL_TSADDREG = 1;
+	while (TSB_EMA_TIMESTAMP_CONTROL_TSADDREG) {
+		k_yield();
+	}
+
+	/* Enable fine timestamp correction method */
+	TSB_EMA_TIMESTAMP_CONTROL_TSCFUPDT = 1;
+
+	/* Enable nanosecond rollover into a new second */
+	TSB_EMA_TIMESTAMP_CONTROL_TSCTRLSSR = 1;
+
+	/* Initialize timestamp */
+	heth->p_instance->SYSTEM_TIME_SECONDS_UPDATE = 0;
+	heth->p_instance->SYSTEM_TIME_NANOSECONDS_UPDATE = 0;
+	
+	TSB_EMA_TIMESTAMP_CONTROL_TSINIT = 1;
+	while (TSB_EMA_TIMESTAMP_CONTROL_TSINIT) {
+		k_yield();
+	}
+
+	return 0;
+}
+
+DEVICE_DEFINE(txzp_ptp_clock_0, PTP_CLOCK_NAME, ptp_txzp_init,
+		NULL, &ptp_txzp_0_context, NULL, POST_KERNEL,
+		CONFIG_ETH_TXZP_PTP_CLOCK_INIT_PRIO, &api);
+
+#endif /* CONFIG_PTP_CLOCK_TXZP */
