+/*--------------------------------------------------*/
+/**
+  * @brief  Set the ethernet MAC address
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance.
+  * @param  macaddr : Pointer to MAC address
+  * @retval -
+  */
+/*--------------------------------------------------*/
+TXZ_Result ethm_set_mac_address(ethm_t *p_obj,uint8_t *macaddr)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+    __IO uint32_t tmpreg;
+    TSB_EM_TypeDef *p_ethm;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)||
+		((void *)(macaddr) == (void *)0)) {
+		 result = TXZ_ERROR;
+
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Store top 16 bits of MAC address */
+	    tmpreg = ((uint32_t)macaddr[5U] << 8U) | (uint32_t)macaddr[4U];
+
+	    /* Write the selected MAC address 0 top register */
+	    p_ethm->MAC_ADDRESS0_HIGH = tmpreg;
+
+	    /* Store lower 32 bits of MAC address */
+	    tmpreg = ((uint32_t)macaddr[3U] << 24U) | ((uint32_t)macaddr[2U] << 16U) | ((uint32_t)macaddr[1U] << 8U) | macaddr[0U];
+
+	    /* Write the selected MAC address 0 lower register */
+	    p_ethm->MAC_ADDRESS0_LOW = tmpreg;
+	}
+    return result;
+
+}
