+/*--------------------------------------------------*/
+/**
+  * @brief  Transmit data
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @param  p_txbuf: Pointer to transmit buffer
+  * @param  frame_len: Pointer to transmit data length
+  * @retval TXZ_SUCCESS :Success
+  * @retval TXZ_ERROR   :Failure
+  */
+/*--------------------------------------------------*/
+TXZ_Result ethm_transmit(ethm_t *p_obj, uint8_t *p_txbuf, uint32_t frame_len)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+	TSB_EM_TypeDef *p_ethm;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0) ||
+		((void *)(p_obj->p_tx_desc) == (void *)0) ||
+		((void *)(p_txbuf) == (void *)0)) {
+
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Check whether send descriptor is owned by the TDMA */
+	    if ((p_obj->p_tx_desc->des0 & TX_DMA_DESC0_OWN) != 0) {
+	        return TXZ_ERROR;
+	    }
+
+	    p_obj->p_tx_desc->des2_Buffer_1_Addr = (uint32_t)p_txbuf;
+
+	    /* Get the number of needed Tx buffers for the current frame */
+	    if (frame_len > ETHM_TX_BUF_SIZE) {
+
+	        return TXZ_ERROR;
+
+	    } else {
+	        /* Set LAST and FIRST segment */
+	        p_obj->p_tx_desc->des0 |= TX_DMA_DESC0_FS | TX_DMA_DESC0_LS;
+	        /* Set frame size */
+	        p_obj->p_tx_desc->des1 = (frame_len & TX_DMA_DES1_TBS1);
+
+	        /* source address insertion control */
+	        p_obj->p_tx_desc->des1 |= TX_DMA_DES1_SAIC;
+
+	        /* Set <OWN> bit.
+				This will cause the descriptor fetch to avoid possible conflict
+				with software that sets the <OWN> bit
+			*/
+	        p_obj->p_tx_desc->des0 |= TX_DMA_DESC0_OWN;
+
+	        /* Set <IC> bit.
+				This will raise transmit interrupt after the current frame has been sent
+			*/
+	        p_obj->p_tx_desc->des0 |= TX_DMA_DESC0_IC;
+
+	        /* Point to next descriptor */
+	        p_obj->p_tx_desc = (TSB_ETHM_DMADesc_TypeDef *)(p_obj->p_tx_desc->des3_Next_Desc_Addr);
+
+	    }
+
+	    /* Check whether software owns the following descriptor in the transmit list */
+	    if ((p_ethm->STATUS & DMA_STATUS_TU) != 0) {
+
+	        /* Clear TU ETHERNET DMA flag to change ownership of the descriptor */
+	        p_ethm->STATUS = DMA_STATUS_TU;
+
+	        /* Resume prcessing transmit descriptor */
+	        p_ethm->TRANSMIT_POLL_DEMAND = TX_POLLDEMAND_TPD;
+
+	    }
+	}
+    return result;
+}
+
+/*--------------------------------------------------*/
+/**
+  * @brief  Receive data
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @param  p_rxbuf: Pointer to receive buffer
+  * @param  p_rxlen: Pointer to receive data length
+  * @retval TXZ_SUCCESS :Success
+  * @retval TXZ_ERROR   :Failure
+  */
+/*--------------------------------------------------*/
+TXZ_Result ethm_receive(ethm_t *p_obj, uint8_t *p_rxbuf, uint32_t *p_rxlen)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+	TSB_EM_TypeDef *p_ethm;
+    uint8_t *buffer;
+    uint32_t len;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0) ||
+		((void *)(p_obj->p_rx_desc) == (void *)0) ||
+		((void *)(p_rxbuf) == (void *)0)) {
+
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Check if the descriptor is owned by the TDMA */
+	    if ((p_obj->p_rx_desc->des0 & RX_DMA_DESC0_OWN) != 0) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Check if first segment and last segment in frame */
+	    if ((p_obj->p_rx_desc->des0 & RX_DMA_DESC0_FS) == (uint32_t)RX_DMA_DESC0_FS) {
+	        if ((p_obj->p_rx_desc->des0 & RX_DMA_DESC0_LS) == (uint32_t)RX_DMA_DESC0_LS) {
+	            *p_rxlen = len = (((p_obj->p_rx_desc)->des0 & RX_DMA_DESC0_FL) >> RX_FRAME_LEN_SHIFT) - CRC_SIZE;
+
+	            /* Get the address of the buffer start address */
+	            buffer = (uint8_t *)(p_obj->p_rx_desc)->des2_Buffer_1_Addr;
+
+				/* copy received data into user buffer */
+	            memcpy((uint8_t *)p_rxbuf, (uint8_t *)buffer, len);
+
+	            p_obj->p_rx_desc->des0 |= RX_DMA_DESC0_OWN;
+
+	            /* Point to next descriptor */
+	            p_obj->p_rx_desc = (TSB_ETHM_DMADesc_TypeDef *)(p_obj->p_rx_desc->des3_Next_Desc_Addr);
+	        }
+
+	        /* When Rx Buffer unavailable flag is set: clear it and resume reception */
+	        if ((p_ethm->STATUS & DMA_STATUS_RU) != 0) {
+	            /* Clear RBUS ETHERNET DMA flag */
+	            p_ethm->STATUS = DMA_STATUS_RU;
+	            /* Resume DMA reception */
+	            p_ethm->RECEIVE_POLL_DEMAND = RX_POLLDEMAND_RPD;
+	        }
+	        /* Return function status */
+	        return TXZ_SUCCESS;
+	    } else {
+	        return TXZ_ERROR;
+	    }
+	}
+
+    return result;
+}
