+				clocks = <&cg TXZP_CG_IPEN_M4N_PL>;
+				reg = <0x400e0a00 0x100>;
+			};
+
+			pm: gpio@400e0b00 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PM>;
+				reg = <0x400e0b00 0x100>;
+			};
+
+			pn: gpio@400e0c00 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PN>;
+				reg = <0x400e0c00 0x100>;
+			};
