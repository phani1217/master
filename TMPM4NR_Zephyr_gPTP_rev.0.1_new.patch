+/*--------------------------------------------------*/
+/**
+  * @brief  Interrupt Handler
+  * @param  - None
+  * @retval - None
+-  */
+/*--------------------------------------------------*/
+void INTEMA_IRQHandler(void)
+{
+    if (ethm_read_register((void *)&TSB_EMA->STATUS) & DMA_STATUS_RI) {
+
+		/*------------------------------*/
+		/*	Call Handler				*/
+		/*------------------------------*/
+		if ((p_EthmObj != ETHM_NULL) &&	(p_EthmObj->rxcallback != ETHM_NULL)) {
+	        p_EthmObj->rxcallback(TXZ_SUCCESS);
+		}
+        /* Clear the receive interrupt flags */
+        ethm_write_register((uint32_t) DMA_STATUS_RI_CLEAR, (void *)&TSB_EMA->STATUS);
+    } else if (ethm_read_register((void *)&TSB_EMA->STATUS) & DMA_STATUS_TI) {
+
+		/*------------------------------*/
+		/*	Call Handler				*/
+		/*------------------------------*/
+		if ((p_EthmObj != ETHM_NULL) &&	(p_EthmObj->txcallback != ETHM_NULL)) {
+	        p_EthmObj->txcallback(TXZ_SUCCESS);
+		}
+        /* Clear the transmit interrupt flags */
+        ethm_write_register((uint32_t) DMA_STATUS_TI_CLEAR, (void *)&TSB_EMA->STATUS);
+    } else if (ethm_read_register((void *)&TSB_EMA->STATUS) & DMA_STATUS_ETI) {
+        /* Clear the early transmit interrupt flags */
+        ethm_write_register((uint32_t) DMA_STATUS_ETI_CLEAR, (void *)&TSB_EMA->STATUS);
+    }
+
+    /* Clear the normal interrupt flags */
+    ethm_write_register((uint32_t) DMA_STATUS_NIS_CLEAR, (void *)&TSB_EMA->STATUS);
+}
