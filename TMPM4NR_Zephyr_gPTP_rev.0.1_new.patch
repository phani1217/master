+
+/*------------------------------------------------------------------------------*/
+/*  Includes                                                                    */
+/*------------------------------------------------------------------------------*/
+#include <zephyr/arch/arm/aarch32/cortex_m/cmsis.h>
+#include <zephyr/device.h>
+#include <zephyr/drivers/clock_control.h>
+#include <zephyr/drivers/uart.h>
+#include <zephyr/logging/log.h>
+#if IS_ENABLED(CONFIG_PINCTRL)
+#include <zephyr/drivers/pinctrl.h>
+#endif
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+#include <zephyr/irq.h>
+#endif
+
+LOG_MODULE_REGISTER(uart_txzp, CONFIG_UART_LOG_LEVEL);
+
+/**
+ * @brief UART TXZP register map structure
+ */
+struct uart_txzp_regs {
+	uint32_t SWRST;             /*!< UART Software Reset Register                 */
+	uint32_t CR0;               /*!< UART Control Register 0                      */
+	uint32_t CR1;               /*!< UART Control Register 1                      */
+	uint32_t CLK;               /*!< UART Clock Control Register                  */
+	uint32_t BRD;               /*!< UART Baud Rate Register                      */
+	uint32_t TRANS;             /*!< UART Transfer Enable Register                */
+	uint32_t DR;                /*!< UART Data Register                           */
+	uint32_t SR;                /*!< UART Status Register                         */
+	uint32_t FIFOCLR;           /*!< UART FIFO Clear Register                     */
+	uint32_t ERR;               /*!< UART Error Register                          */
+};
+
+/**
+ * @brief Device configuration structure definition
+ *
+ * UART controller configuration for the given device
+ *
+ */
+struct uart_txzp_config {
+	volatile struct uart_txzp_regs *base;
+	uint32_t clkid;
+	const struct device *clk_dev;
+#if IS_ENABLED(CONFIG_PINCTRL)
+	const struct pinctrl_dev_config *pincfg;
+#endif
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+	uart_irq_config_func_t	irq_config_func;
+#endif
+};
+
+/**
+ * @brief UART device run-time data structure definition
+ */
+struct uart_txzp_data {
+	uint32_t baud_rate;	/* Baud rate */
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+	uart_irq_callback_user_data_t irq_cb;
+	void *irq_cb_data;
+#endif
+};
+
+#define UART_SWRST_01	BIT(0)
+#define UART_SWRST_10	BIT(1)
+#define UART_SWRSTF		BIT(7)
+
+#define UART_FIFOCLR_TFCLR_CLEAR	BIT(1)
+#define UART_FIFOCLR_RFCLR_CLEAR	BIT(0)
+
+
+#define UART_SR_TXRUN			BIT(15)
+#define UART_SR_TXEND			BIT(14)
+#define UART_SR_TLVL_MAX		BIT(11)
+#define UART_SR_TLVL			(BIT(11)|BIT(10)|BIT(9)|BIT(8))
+#define uART_SR_RXEND			BIT(6)
+#define UART_SR_RLVL			(BIT(3)|BIT(2)|BIT(1)|BIT(0))
+
+#define UART_BRD_KEN			BIT(23)
+#define UART_BRK_WIDTH			6u
+#define UART_BRN_WIDTH			16
+
+#define UART_CR0_SBLEN_2		BIT(4)
+#define UART_CR0_EVEN			BIT(3)
+#define UART_CR0_PE				BIT(2)
+#define UART_CR0_SM_7			0u
+#define UART_CR0_SM_8			1u
+#define UART_CR0_SM_9			2u
+
+#define UART_CR1_INTTXFE		BIT(7)
+#define UART_CR1_INTTXWE		BIT(6)
+#define UART_CR1_INTRXFE		BIT(5)
+#define UART_CR1_INTRXWE		BIT(4)
+#define UART_CR1_INTERR			BIT(2)
+
+#define UART_ERR_TRGERR			BIT(4)
+#define UART_ERR_OVRERR			BIT(3)	/* overrun error */
+#define UART_ERR_PERR			BIT(2)	/* parity error */
+#define UART_ERR_FERR			BIT(1)	/* framing error */
+#define UART_ERR_BERR			BIT(0)	/* break error */
+
+#define UART_TRANS_TXE		BIT(1)	/* transmit enable */
+#define UART_TRANS_RXE		BIT(0)	/* receive enable */
+
+#define UART_SR_SUE			BIT(31)
+
+/**
+ * @brief Resets the uart
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_software_reset(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	/* Wait for idle */
+	while (config->base->SWRST & UART_SWRSTF) {
+		; /* Wait */
+	}
+	config->base->SWRST = UART_SWRST_10;
+	config->base->SWRST = UART_SWRST_01;
+	/* Wait for idle */
+	while (config->base->SWRST & UART_SWRSTF) {
+		; /* Wait */
+	}
+}
+
+/**
+ * @brief Sets the baud rate
+ * @param dev UART device driver instance
+ * @param clk Baud rate division
+ * @param baudrate Value of baudrate to set
+ * @return 0 success
+ * @return else error
+ */
+static int uart_txzp_set_baudrate(const struct device *dev,
+			      uint32_t clk, uint32_t baudrate)
+{
+	const struct uart_txzp_config *config = dev->config;
+	/* Avoiding float calculations, bauddiv is left shifted by 6 */
+	uint64_t bauddiv = (((uint64_t)clk) << UART_BRK_WIDTH)
+				/ (baudrate * 16U);
+
+	/* Valid bauddiv value
+	 * uart_clk (min) >= 16 x baud_rate (max)
+	 * uart_clk (max) <= 16 x 65535 x baud_rate (min)
+	 */
+	if ((bauddiv < (1u << UART_BRK_WIDTH))
+		|| (bauddiv > (65535u << UART_BRK_WIDTH))) {
+		return -EINVAL;
+	}
+
+	uint32_t brn = (bauddiv >> UART_BRK_WIDTH);
+	uint32_t brk = bauddiv & ((1u << UART_BRK_WIDTH) - 1u);
+	if (brk) {
+		config->base->BRD = UART_BRD_KEN | (brk << UART_BRN_WIDTH) | brn;
+	} else {
+		config->base->BRD = brn;
+	}
+	__DMB();
+
+	return 0;
+}
+
+/**
+ * @brief Check whether fifo is readable
+ *
+ * Check the count of receive FIFO.
+ *
+ * @param dev UART device driver instance
+ * @retval true readable
+ * @retval false not readable, FIFO empty
+ */
+static bool uart_txzp_is_readable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	if (config->base->SR & UART_SR_RLVL) {
+		return true;
+	}
+	return false;
+}
+
+/**
+ * @brief Gets data from receive FIFO
+ *
+ * This function is polling based.
+ *
+ * @param dev UART device driver instance
+ * @param c Pointer to variable where received character would be stored
+ * @retval 0 Success
+ * @retval -1 Failure
+ */
+static int uart_txzp_poll_in(const struct device *dev, unsigned char *c)
+{
+	const struct uart_txzp_config *config = dev->config;
+	if (!uart_txzp_is_readable(dev)) {
+		return -1;
+	}
+	/* got a character */
+	*c = (unsigned char)config->base->DR;
+	return 0;
+}
+
+/**
+ * @brief Sets data into transmit FIFO
+ * @param dev UART device driver instance
+ * @param c data to be transmitted
+ */
+static void uart_txzp_poll_out(const struct device *dev, unsigned char c)
+{
+	const struct uart_txzp_config *config = dev->config;
+	/* Wait for space in FIFO */
+	while (config->base->SR & UART_SR_TLVL_MAX) {
+		; /* Wait */
+	}
+	/* Send a character */
+	config->base->DR = (uint32_t)c;
+}
+
+/**
+ * @brief Checks error
+ * @param dev UART device driver instance
+ * @retval 0 No error
+ * @retval Bitwise OR of all the occurred error
+ */
+static int uart_txzp_err_check(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	uint32_t err = config->base->ERR;
+	int result = 0;
+	result |= (err & UART_ERR_OVRERR) ? UART_ERROR_OVERRUN: 0;
+	result |= (err & UART_ERR_PERR) ? UART_ERROR_PARITY: 0;
+	result |= (err & UART_ERR_FERR) ? UART_ERROR_FRAMING: 0;
+	result |= (err & UART_ERR_BERR) ? UART_BREAK: 0;
+	return result;
+}
+
+/**
+ * @brief Configures UART
+ * @param dev UART device driver instance
+ * @param cfg UART configuration
+ * @retval 0 Success
+ * @retval <0 Error
+ */
+static int uart_txzp_configure(const struct device *dev, const struct uart_config *cfg)
+{
+	const struct uart_txzp_config *config = dev->config;
+	int ret;
+	uint32_t rate;
+	uint32_t settings = 0;
+
+	if (config->base->SR & UART_SR_SUE) {
+		return -EBUSY;
+	}
+
+	ret = clock_control_get_rate(config->clk_dev,
+								(clock_control_subsys_t *)&config->clkid,
+								&rate);
+	if (ret < 0) {
+		return ret;
+	}
+	ret = uart_txzp_set_baudrate(dev, rate, cfg->baudrate);
+	if (ret < 0) {
+		return ret;
+	}
+	switch (cfg->parity) {
+	case UART_CFG_PARITY_NONE:
+		break;
+	case UART_CFG_PARITY_ODD:
+		settings |= UART_CR0_PE;
+		break;
+	case UART_CFG_PARITY_EVEN:
+		settings |= UART_CR0_PE | UART_CR0_EVEN;
+		break;
+	default:
+		return -EINVAL;
+	}
+	switch (cfg->stop_bits) {
+	case UART_CFG_STOP_BITS_1:
+		break;
+	case UART_CFG_STOP_BITS_2:
+		settings |= UART_CR0_SBLEN_2;
+		break;
+	default:
+		return -EINVAL;
+	}
+	switch (cfg->data_bits) {
+	case UART_CFG_DATA_BITS_7:
+		break;
+	case UART_CFG_DATA_BITS_8:
+		settings |= UART_CR0_SM_8;
+		break;
+	case UART_CFG_DATA_BITS_9:
+		settings |= UART_CR0_SM_9;
+		break;
+	default:
+		return -EINVAL;
+	}
+	switch (cfg->flow_ctrl) {
+	case UART_CFG_FLOW_CTRL_NONE:
+		break;
+	default:
+		return -EINVAL;
+	}
+	config->base->CR0 = settings;
+	return 0;
+}
+
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+/**
+ * @brief Sets data into transmit FIFO
+ *
+ * This function is interrupt based.
+ *
+ * @param dev UART device driver instance
+ * @param tx_data Pointer to the variable where data to be transmitted is stored
+ * @param len Length of data
+ * @retval length of data transmitted
+ */
+static int uart_txzp_fifo_fill(const struct device *dev,
+				    const uint8_t *tx_data, int len)
+{
+	const struct uart_txzp_config *config = dev->config;
+	uint8_t num_tx = 0U;
+
+	while (!(config->base->SR & UART_SR_TLVL_MAX) && (len - num_tx > 0)) {
+		config->base->DR = tx_data[num_tx++];
+	}
+	return num_tx;
+}
+
+/**
+ * @brief Gets data from receive FIFO
+ *
+ * This function is interrupt based.
+ *
+ * @param dev UART device driver instance
+ * @param rx_data Pointer to variable where received data would be stored
+ * @param len len of received data
+ * @retval 0 length of data received
+ */
+static int uart_txzp_fifo_read(const struct device *dev,
+				    uint8_t *rx_data, const int len)
+{
+	const struct uart_txzp_config *config = dev->config;
+	uint8_t num_rx = 0U;
+
+	while ((len - num_rx > 0) && (config->base->SR & UART_SR_RLVL)) {
+		rx_data[num_rx++] = config->base->DR;
+	}
+
+	return num_rx;
+}
+
+/**
+ * @brief Enables the transmit interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_tx_enable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	config->base->CR1 |= UART_CR1_INTTXWE;
+}
+
+/**
+ * @brief Disables the transmit interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_tx_disable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	config->base->CR1 &= ~UART_CR1_INTTXWE;
+}
+
+/**
+ * @brief Check the transmit completion
+ * @param dev UART device driver instance
+ * @retval 1 Transmitting
+ * @retval 0 Transmission Complete
+ */
+static int uart_txzp_irq_tx_complete(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	if ((UART_SR_TXRUN | UART_SR_TLVL) & config->base->SR) {
+		return 1;
+	}
+	return 0;
+}
+
+
+/**
+ * @brief Check if UART TX buffer can accept a new char
+ * @param dev UART device driver instance
+ * @retval 1 Atleast 1 char can be written to UART
+ * @retval 0 If device is not ready to write a new byte
+ */
+static int uart_txzp_irq_tx_ready(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+
+	if ((config->base->CR1 & UART_CR1_INTTXWE) &&
+		!(config->base->SR & UART_SR_TLVL_MAX)) {
+		return 1;
+	}
+	return 0;
+}
+
+/**
+ * @brief Enables the receive interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_rx_enable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	config->base->CR1 |= UART_CR1_INTRXWE;
+}
+
+/**
+ * @brief Disables the receive interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_rx_disable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	config->base->CR1 &= ~UART_CR1_INTRXWE;
+}
+
+/**
+ * @brief Check if UART RX buffer has a received char
+ * @param dev UART device driver instance
+ * @retval 1 If a received char is ready.
+ * @retval 0 If a received char is not ready
+ */
+static int uart_txzp_irq_rx_ready(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+
+	if ((config->base->CR1 & UART_CR1_INTRXWE) &&
+		(config->base->SR & UART_SR_RLVL)) {
+		return 1;
+	}
+	return 0;
+}
+
+/**
+ * @brief Enables the error interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_err_enable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	/* enable framing, parity, break, and overrun */
+	config->base->CR1 |= UART_CR1_INTERR;
+}
+
+/**
+ * @brief Disables the error interrupt
+ * @param dev UART device driver instance
+ */
+static void uart_txzp_irq_err_disable(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	config->base->CR1 &= ~UART_CR1_INTERR;
+}
+
+/**
+ * @brief Check whether transmit/receive Interrupt is pending
+ * @param dev UART device driver instance
+ * @retval 1 pending
+ * @retval 0 Not pending
+ */
+static int uart_txzp_irq_is_pending(const struct device *dev)
+{
+	return uart_txzp_irq_rx_ready(dev) || uart_txzp_irq_tx_ready(dev);
+}
+
+/**
+ * @brief Update interrupt
+ * @param dev UART device driver instance
+ * @retval 1
+ */
+static int uart_txzp_irq_update(const struct device *dev)
+{
+	return 1;
+}
+
+/**
+ * @brief Set interrupt callback
+ * @param dev UART device driver instance
+ * @param cb Pointer to callback function
+ * @param cb_data Pointer to the callback data
+ */
+static void uart_txzp_irq_callback_set(const struct device *dev,
+					    uart_irq_callback_user_data_t cb,
+					    void *cb_data)
+{
+	struct uart_txzp_data *data = dev->data;
+
+	data->irq_cb = cb;
+	data->irq_cb_data = cb_data;
+}
+#endif /* CONFIG_UART_INTERRUPT_DRIVEN */
+
+/**
+ * @brief Gets the APIs, the uart driver will export
+ *
+ * API to transmit/receive polling based
+ * API to transmit/receive interrupt based
+ * API to configure uart
+ * API to enable tx/rx interrupts
+ * API to check tx/rx completion and ready state
+ * API to set callback
+ *
+ */
+static const struct uart_driver_api uart_txzp_driver_api = {
+	.poll_in = uart_txzp_poll_in,
+	.poll_out = uart_txzp_poll_out,
+	.err_check = uart_txzp_err_check,
+	.configure = uart_txzp_configure,
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+	.fifo_fill = uart_txzp_fifo_fill,
+	.fifo_read = uart_txzp_fifo_read,
+	.irq_tx_enable = uart_txzp_irq_tx_enable,
+	.irq_tx_disable = uart_txzp_irq_tx_disable,
+	.irq_tx_ready = uart_txzp_irq_tx_ready,
+	.irq_rx_enable = uart_txzp_irq_rx_enable,
+	.irq_rx_disable = uart_txzp_irq_rx_disable,
+	.irq_tx_complete = uart_txzp_irq_tx_complete,
+	.irq_rx_ready = uart_txzp_irq_rx_ready,
+	.irq_err_enable = uart_txzp_irq_err_enable,
+	.irq_err_disable = uart_txzp_irq_err_disable,
+	.irq_is_pending = uart_txzp_irq_is_pending,
+	.irq_update = uart_txzp_irq_update,
+	.irq_callback_set = uart_txzp_irq_callback_set,
+#endif /* CONFIG_UART_INTERRUPT_DRIVEN */
+};
+
+/**
+ * @brief Initializes the UART
+ * @param dev UART device driver instance
+ * @retval 0 Success
+ * @retval <0 Error
+ */
+static int uart_txzp_init(const struct device *dev)
+{
+	const struct uart_txzp_config *config = dev->config;
+	struct uart_txzp_data *data = dev->data;
+	uint32_t rate;
+	int ret;
+
+	if (!device_is_ready(config->clk_dev)) {
+		return -ENODEV;
+	}
+
+	ret = clock_control_on(config->clk_dev, (clock_control_subsys_t *)&config->clkid);
+	if (ret < 0) {
+		LOG_ERR("Failed call clock_control_on: return [%d]", ret);
+		return -EIO;
+	}
+	ret = clock_control_get_rate(config->clk_dev,
+								(clock_control_subsys_t *)&config->clkid,
+								&rate);
+	if (ret != 0) {
+		LOG_ERR("Failed call clock_control_get_rate: return [%d]", ret);
+		return -EIO;
+	}
+#if IS_ENABLED(CONFIG_PINCTRL)
+	ret = pinctrl_apply_state(config->pincfg, PINCTRL_STATE_DEFAULT);
+	if (ret) {
+		return ret;
+	}
+#endif
+
+	uart_txzp_software_reset(dev);
+	config->base->FIFOCLR = UART_FIFOCLR_TFCLR_CLEAR | UART_FIFOCLR_RFCLR_CLEAR;
+
+	/* Set baud rate */
+	ret = uart_txzp_set_baudrate(dev, rate, data->baud_rate);
+	if (ret != 0) {
+		return ret;
+	}
+
+	/* Setting the default character format */
+	config->base->CR0 = UART_CR0_SM_8;
+
+	__ISB();
+	config->base->TRANS = UART_TRANS_RXE | UART_TRANS_TXE;
+	__ISB();
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+	config->irq_config_func(dev);
+#endif
+	return 0;
+}
+
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+
+/**
+ * @brief ISR to process interrupts
+ * @param dev UART device driver instance
+ *
+ */
+void uart_txzp_isr(const struct device *dev)
+{
+	struct uart_txzp_data *data = dev->data;
+	/* Verify if the callback has been registered */
+	if (data->irq_cb) {
+		data->irq_cb(dev, data->irq_cb_data);
+	}
+}
+#endif
+
+#if IS_ENABLED(CONFIG_PINCTRL)
+#define PINCTRL_DEFINE(n) PINCTRL_DT_INST_DEFINE(n);
+#define PINCTRL_INIT(n) .pincfg = PINCTRL_DT_INST_DEV_CONFIG_GET(n),
+#else
+#define PINCTRL_DEFINE(n)
+#define PINCTRL_INIT(n)
+#endif /* CONFIG_PINCTRL */
+
+
+#ifdef CONFIG_UART_INTERRUPT_DRIVEN
+#define UART_TXZP_IRQ_CONFIG_FUNC_BODY(n, prop, i)		\
+	{						\
+		IRQ_CONNECT(DT_IRQ_BY_IDX(n, i, irq),	\
+			DT_IRQ_BY_IDX(n, i, priority),	\
+			uart_txzp_isr,			\
+			DEVICE_DT_GET(n),		\
+			0);				\
+		irq_enable(DT_IRQ_BY_IDX(n, i, irq));	\
+	}
+
+#define UART_IRQ_CONFIG_FUNC(n)					\
+	static void uart_irq_config_func_##n(const struct device *dev)	\
+	{													\
+		DT_INST_FOREACH_PROP_ELEM(n, interrupt_names,	\
+			UART_TXZP_IRQ_CONFIG_FUNC_BODY)				\
+	}
+
+#define UART_IRQ_CONFIG_FUNC_INIT(n)					\
+	.irq_config_func = uart_irq_config_func_##n,
+
+#else
+#define UART_IRQ_CONFIG_FUNC(n)
+#define UART_IRQ_CONFIG_FUNC_INIT(n)
+#endif /* CONFIG_UART_INTERRUPT_DRIVEN */
+
+#define UART_TXZP_CONFIG_PORT(n)								\
+	static struct uart_txzp_config uart_txzp_cfg_port_##n = {				\
+		.base = (volatile struct uart_txzp_regs *)DT_INST_REG_ADDR(n),	\
+		.clkid = DT_INST_CLOCKS_CELL(n, clkid),							\
+		.clk_dev = DEVICE_DT_GET(DT_INST_CLOCKS_CTLR(n)),				\
+		PINCTRL_INIT(n)	\
+		UART_IRQ_CONFIG_FUNC_INIT(n)	\
+	};
+
+#define UART_TXZP_INIT(n)		\
+	PINCTRL_DEFINE(n)			\
+	UART_IRQ_CONFIG_FUNC(n)		\
+	UART_TXZP_CONFIG_PORT(n)	\
+								\
+	static struct uart_txzp_data uart_txzp_data_port_##n = {	\
+		.baud_rate = DT_INST_PROP(n, current_speed),	\
+	};							\
+								\
+	DEVICE_DT_INST_DEFINE(n, &uart_txzp_init,			\
+			NULL,					\
+			&uart_txzp_data_port_##n,			\
+			&uart_txzp_cfg_port_##n,			\
+			PRE_KERNEL_1,				\
+			CONFIG_SERIAL_INIT_PRIORITY,		\
+			&uart_txzp_driver_api);
+
+DT_INST_FOREACH_STATUS_OKAY(UART_TXZP_INIT)
