+/**
+ ***********************************************************************
+ * Copyright (c) 2016 Open-RnD Sp. z o.o.
+ * Copyright (c) 2021 Linaro Limited
+ * Copyright (c) 2021 Nordic Semiconductor ASA
+ * Copyright (c) 2023 Toshiba Electronic Devices & Storage Corporation
+ *
+ * TXZ+ Pinctrl driver
+ *
+ * SPDX-License-Identifier: Apache-2.0
+ ************************************************************************
+ */
+/*------------------------------------------------------------------------------*/
+/*  Macro Definition                                                            */
+/*------------------------------------------------------------------------------*/
+#define DT_DRV_COMPAT toshiba_txzp_pinctrl
+
+/*------------------------------------------------------------------------------*/
+/*  Includes                                                                    */
+/*------------------------------------------------------------------------------*/
+#include <zephyr/drivers/pinctrl.h>
+#include <gpio/gpio_txzp.h>
+
+static const struct device *const txzp_ports[] = {
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pa)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pb)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pc)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pd)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pe)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pf)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pg)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(ph)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pj)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pk)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pl)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pm)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pn)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pp)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pr)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pt)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pu)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pv)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(pw)),
+	DEVICE_DT_GET_OR_NULL(DT_NODELABEL(py)),
+};
+
+/**
+ * @brief Configure the pin
+ * @param pin Pin to be configured
+ * @return 0 success
+ * @return else error
+ */
+static int pinctrl_configure_pin(const pinctrl_soc_pin_t *pin)
+{
+	const struct device *port_device;
+	if (pin->port >= ARRAY_SIZE(txzp_ports)) {
+		return -EINVAL;
+	}
+	port_device = txzp_ports[pin->port];
+	if ((port_device == NULL) || (!device_is_ready(port_device))) {
+		return -ENODEV;
+	}
+	gpio_txzp_set_function(port_device, pin->pin, pin->function);
+	uint32_t flags = 0;
+	flags |= pin->input_enable ? GPIO_INPUT: 0;
+	flags |= pin->output_enable ? GPIO_OUTPUT: 0;
+	flags |= pin->open_drain ? GPIO_LINE_OPEN_DRAIN: 0;
+	flags |= pin->pull_down ? GPIO_PULL_DOWN: 0;
+	flags |= pin->pull_up ? GPIO_PULL_UP: 0;
+	gpio_txzp_pin_configure(port_device, pin->pin, flags);
+	return 0;
+}
+
+/**
+ * @brief Configure the set of pins
+ * @param pins List of pins to be configured
+ * @param pin_cnt Number of pins
+ * @param reg Device register
+ * @return 0 success
+ * @return else error
+ */
+int pinctrl_configure_pins(const pinctrl_soc_pin_t *pins, uint8_t pin_cnt,
+			   uintptr_t reg)
+{
+	ARG_UNUSED(reg);
+
+	for (uint8_t i = 0U; i < pin_cnt; i++) {
+		int ret = pinctrl_configure_pin(pins++);
+		if (ret < 0) {
+			return ret;
+		}
+	}
+
+	return 0;
+}
