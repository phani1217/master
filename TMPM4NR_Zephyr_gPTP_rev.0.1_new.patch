+/*--------------------------------------------------*/
+/**
+  * @brief  Configures and initialize DMA.
+  * @param  p_obj : Structure handle contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @retval TXZ_SUCCESS :Success.
+  * @retval TXZ_ERROR   :Failure.
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_dma_init(ethm_t *p_obj)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+	TSB_EM_TypeDef *p_ethm;
+    TSB_ETHM_DMA_Config_TypeDef dma_config;
+    __IO uint32_t tmpreg = 0U;
+
+	/* Check for NULL pointer */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /* Default DMA Initilization */
+	    dma_config.DT = OPEMODE_DISCARD_TCPIP_CHECKSUM_ERR_FRAME_ENABLE;
+	    dma_config.RSF = OPEMODE_RECEIVE_SAVE_TRANSFER_ENABLE;
+	    dma_config.TSF = OPEMODE_TRANSMIT_STORE_FORWARD_ENABLE;
+	    dma_config.TTC = OPEMODE_TRANSMIT_THRESHOLD_CONTROL_64BYTES;
+	    dma_config.FEF = OPEMODE_TRANSFER_ERROR_FRAME_DISABLE;
+	    dma_config.FUF = OPEMODE_TRANSFER_UNDER_SIZE_GOOD_FRAMES_DISABLE;
+	    dma_config.DGF = OPEMODE_JUMBO_FRAME_DISCARD_ENABLE;
+	    dma_config.RTC = OPEMODE_RECEIVE_THRESHOLD_CONTROL_64BYTES;
+	    dma_config.RPBL = BUSMODE_RPBL_32BEAT;
+	    dma_config.USP  = BUSMODE_ISOLATED_PBL_ENABLE;
+	    dma_config.PBL = BUSMODE_PBL_32BEAT;
+	    dma_config.ATDS = BUSMODE_DESCRIPTOR_SIZE_32BYTES;
+	    dma_config.DSL = BUSMODE_INTER_DESCRIPTOR_SKIP_LEN;
+
+	    /* Read the Operational Mode Register */
+		    tmpreg = p_ethm->OPERATION_MODE;
+
+	    /* Assign the operational mode config values */
+	    tmpreg |= (uint32_t)(dma_config.DT |
+	                         dma_config.RSF |
+	                         dma_config.TSF |
+	                         dma_config.TTC |
+	                         dma_config.FUF);
+
+	    /* Set values to Operational Mode Register */
+		    p_ethm->OPERATION_MODE = tmpreg;
+
+	    /* Set values to Bus Mode Register */
+		    p_ethm->BUS_MODE = dma_config.RPBL | dma_config.PBL | dma_config.DSL | dma_config.USP | dma_config.ATDS;
+
+	    /* Transmit descriptor initialization*/
+	    if (ethm_dma_txdesc_init(p_obj) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+	    /* Receive descriptor initialization*/
+	    if (ethm_dma_rxdesc_init(p_obj) != TXZ_SUCCESS) {
+	        return TXZ_ERROR;
+	    }
+
+	    /* Enable the DMA transmission */
+		    p_ethm->OPERATION_MODE |= OPEMODE_TX_INTERRUPT_ENABLE;
+
+	    /* Enable the DMA reception */
+		    p_ethm->OPERATION_MODE |= OPEMODE_RX_INTERRUPT_ENABLE;
+
+	    /* Allow Receive Interrupt */
+	    TSB_EMA_INTERRUPT_ENABLE_RIE = 1;
+
+	    /* Allow Transmit Interrupt */
+	    TSB_EMA_INTERRUPT_ENABLE_TIE = 1;
+
+	    TSB_EMA_INTERRUPT_ENABLE_NIE = 1;
+	}
+    return result;
+}
