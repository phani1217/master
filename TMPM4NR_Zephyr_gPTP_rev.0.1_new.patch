+/*--------------------------------------------------*/
+/**
+  * @brief  Initialize the DMA Transmit descriptors.
+  * @param  p_obj : Structure that contains
+  *         ethernet module instance, transmit and receive descriptors information.
+  * @retval TXZ_SUCCESS :Success.
+  * @retval TXZ_ERROR   :Failure.
+  */
+/*--------------------------------------------------*/
+static TXZ_Result ethm_dma_txdesc_init(ethm_t *p_obj)
+{
+    TXZ_Result result = TXZ_SUCCESS;
+    uint8_t index = 0U;
+    uint8_t *p_txbuf;
+    TSB_ETHM_DMADesc_TypeDef *p_dmatxdesc,*p_dmatxdesc_next;
+	TSB_EM_TypeDef *p_ethm;
+
+    /* Check the parameter.           */
+	if (((void *)(p_obj) == (void *)0) ||
+		((void *)(p_obj->p_instance) == (void *)0)) {
+		 result = TXZ_ERROR;
+	 } else {
+
+		p_ethm = p_obj->p_instance;
+
+	    /*Assign to transmit buffer base address */
+	    p_txbuf = &ethm_txBuffer[0][0];
+
+	    /* Assign pointer to first linked descriptor */
+	    p_obj->p_tx_desc = ethm_txDescriptor;
+
+	    /* Loop all descriptors and assign valid buffer address and  descriptor address */
+	    for (index = 0; index < ETHM_TX_DESCRIPTOR_NUM; index++) {
+	        /* Assign temp pointer to descriptor present at index location */
+	        p_dmatxdesc = p_obj->p_tx_desc + index;
+			p_dmatxdesc_next = p_dmatxdesc + 1;
+
+	        /* Set Second Address Chain bit. Indicates second address of the descriptor is the next descriptor address*/
+	        p_dmatxdesc->des0 = TX_DMA_DESC0_TCH;
+
+	        /* Assign Send buffer 1 address pointer to transmit buffer*/
+	        p_dmatxdesc->des2_Buffer_1_Addr = (uint32_t)(&p_txbuf[index * ETHM_TX_BUF_SIZE]);
+
+	        /* Assign descriptor address to Next descriptor address */
+	        if (index < ETHM_LAST_TX_DESCRIPTOR) {
+	            /* If current descriptor is not last descriptor,
+					write next descriptor address value to next descriptor base address */
+	            p_dmatxdesc->des3_Next_Desc_Addr = (uint32_t)p_dmatxdesc_next;
+	        } else {
+	            /* If current descriptor is last descriptor,
+					write next descriptor address register to the first descriptor base address */
+	            p_dmatxdesc->des3_Next_Desc_Addr = (uint32_t) p_obj->p_tx_desc;
+	        }
+	    }
+
+	    /* Assign start address of the send descriptor list */
+	    p_ethm->TRANSMIT_DESCRIPTOR_LIST_ADDRESS = (uint32_t) ethm_txDescriptor;
+	}
+    return result;
+}
+
