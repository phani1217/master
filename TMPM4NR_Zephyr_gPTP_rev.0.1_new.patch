+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e0f00 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PT>;
+			};
+
+			pu: gpio@400e1000 {
+				compatible = "toshiba,txzp-gpio";
+				gpio-controller;
+				#gpio-cells = <2>;
+				reg = <0x400e1000 0x100>;
+				clocks = <&cg TXZP_CG_IPEN_M4N_PU>;
